<o:SObjTargetItemText Id="o143">
<a:Name>SqlListQuery</a:Name>
<a:TEXTITEM.Value>{TABLE, OWNER, @OBJTCODE, ISPKEY, COLUMNS}

select
   o.name,
   u.name,
   i.name,
   i.indid,
   isnull(index_col(o.name,i.indid,1),&#39;&#39;) + &#39;,&#39; +
   isnull(index_col(o.name,i.indid,2),&#39;&#39;) + &#39;,&#39; +
   isnull(index_col(o.name,i.indid,3),&#39;&#39;) + &#39;,&#39; +
   isnull(index_col(o.name,i.indid,4),&#39;&#39;) + &#39;,&#39; +
   isnull(index_col(o.name,i.indid,5),&#39;&#39;) + &#39;,&#39; +
   isnull(index_col(o.name,i.indid,6),&#39;&#39;) + &#39;,&#39; +
   isnull(index_col(o.name,i.indid,7),&#39;&#39;) + &#39;,&#39; +
   isnull(index_col(o.name,i.indid,8),&#39;&#39;)
from
sysindexes i,
sysusers u,
sysobjects o
where o.id=i.id
   and u.uid=o.uid
   [and o.name=&#39;%TABLE%&#39;]
   [and u.name=&#39;%OWNER%&#39;]
   [and i.name=&#39;%INDEX%&#39;]
   and 
      ( (i.status&amp;2=2)
      or (i.indid=1) )
order by
   o.name,
   u.name,
   i.name,
   i.indid</a:TEXTITEM.Value>
<a:TRGTITEM.Comment>SQL query to list objects</a:TRGTITEM.Comment>
</o:SObjTargetItemText>
