<a:INTITEM.Value>128</a:INTITEM.Value>
</o:SObjTargetItemInt>
<o:SObjTargetItemBool Id="o83">
<a:Name>EnableNull</a:Name>
<a:TRGTITEM.Comment>NULL keyword allowed</a:TRGTITEM.Comment>
<a:BOOLITEM.Value>1</a:BOOLITEM.Value>
</o:SObjTargetItemBool>
<o:SObjTargetItemBool Id="o84">
<a:Name>EnableIdentity</a:Name>
<a:TRGTITEM.Comment>Identity keyword entry support. Identity columns are serial counters maintains by the database</a:TRGTITEM.Comment>
<a:BOOLITEM.Value>1</a:BOOLITEM.Value>
