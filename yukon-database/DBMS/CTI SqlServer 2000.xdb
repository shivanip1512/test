<a:TEXTITEM.Value>{OWNER, TABLE, TRIGGER, TRGEVENT}

select 
   u.name,   tb.name,   tr.name,   &#39;delete&#39;
from
   sysusers u,   sysobjects tb,   sysobjects tr
where
   u.uid = tr.uid and
   tr.uid=tb.uid
   and tb.deltrig=tr.id
   and tr.type=&#39;TR&#39;
   [and u.name=&#39;%SCHEMA%&#39; and tb.uid=u.uid]
union
select 
   u.name,   tb.name,   tr.name,   &#39;insert&#39;
from
   sysusers u,   sysobjects tb,   sysobjects tr
where
   u.uid = tr.uid and
   tr.uid=tb.uid
   and tb.instrig=tr.id
   and tr.type=&#39;TR&#39;
   [and u.name=&#39;%SCHEMA%&#39; and tb.uid=u.uid]
union
select 
   u.name,   tb.name,   tr.name,   &#39;update&#39;
from
   sysusers u,   sysobjects tb,   sysobjects tr
where
   u.uid = tr.uid and
   tr.uid=tb.uid
   and tb.updtrig=tr.id
   and tr.type=&#39;TR&#39;
   [and u.name=&#39;%SCHEMA%&#39; and tb.uid=u.uid]
</a:TEXTITEM.Value>
<a:TRGTITEM.Comment>SQL query to list objects</a:TRGTITEM.Comment>
</o:SObjTargetItemText>
<o:SObjTargetItemText Id="o203">
<a:Name>SqlAttrQuery</a:Name>
<a:TEXTITEM.Value>{OWNER ID , TABLE ID, TRIGGER ID, SCRIPT ...}

select
   u.name,
   tb.name,
   tr.name,
   cm.text
from
   sysusers u,
   sysobjects tb,
   sysobjects tr,
   syscomments cm
where
   cm.id=tr.id
   and u.uid=tr.uid
   and (tb.deltrig=tr.id or tb.instrig=tr.id or tb.updtrig=tr.id)
   and (tr.deltrig!=0 or tr.instrig!=0 or tr.updtrig!=0)
   [and u.name=&#39;%SCHEMA%&#39;]
   [and tb.name=&#39;%TABLE%&#39;]
   [and tr.name=&#39;%TRIGGER%&#39;]
order by
   tb.name,
   tr.name,
   cm.number,
   cm.colid

</a:TEXTITEM.Value>
<a:TRGTITEM.Comment>SQL query to reverse object attributes</a:TRGTITEM.Comment>
</o:SObjTargetItemText>
