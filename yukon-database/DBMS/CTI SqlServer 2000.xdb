<a:Name>SqlListQuery</a:Name>
<a:TEXTITEM.Value>{@OBJTCODE id, DTTPCODE, LENGTH, PREC, ISMAND} 

select distinct
   u.name,
   i.name,
   isnull(u.prec,u.length),
   u.scale,
   u.allownulls
from
   dbo.systypes u,
   dbo.systypes i
where
   u.type=i.type
   and u.usertype&gt;=100
   and i.usertype&lt;100
   and i.usertype not in (0, 18)
order by
