<o:TextTargetItem Id="o323">
<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, ADT, TYPE, SIZE, DTTPCODE, LENGTH, PREC}

select
   o.owner,
   o.type_name,
   decode(o.incomplete, &#39;YES&#39;, &#39;&#39;, decode(o.typecode, &#39;COLLECTION&#39;, decode(c.coll_type, &#39;VARYING ARRAY&#39;, &#39;VARRAY&#39;, c.coll_type), decode(j.external_name, &#39;&#39;, &#39;OBJECT&#39;, &#39;SQLJ_OBJECT&#39;))),
   c.upper_bound,
   c.elem_type_name,
   c.length,
   c.precision
from
   all_types o,
   all_coll_types c, 
   all_sqlj_types j
where 1 = 1
   and (c.owner(+)=o.owner and c.type_name(+)=o.type_name)
   and (j.owner(+)=o.owner and j.type_name(+)=o.type_name)
   and o.owner is not null
[  and o.owner=%.q:SCHEMA%]
order by 1, 2
</a:TextTargetItem.Value>
<a:Comment>SQL query to list objects</a:Comment>
</o:TextTargetItem>
