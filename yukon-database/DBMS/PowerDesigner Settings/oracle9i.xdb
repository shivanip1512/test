<a:CheckGlobalScript>&#39;******************************************************************************
&#39;* Purpose:  This VB-Script holds global definitions shared by all the custom-
&#39;*            checks scripts of the model extension.
&#39;******************************************************************************

Option Explicit &#39; This is to ensure all used variables are defined

function checkJidxNamespace(folder, obj)
   dim tabl
   dim ret
   checkJidxNamespace = True
   for each tabl in folder.Tables
      If Not tabl.IsShortcut Then
         checkJidxNamespace = checkJidx(obj, tabl)
         if (checkJidxNamespace = False) then
            Exit Function
         end if
      end if
   Next
   dim fold

   Dim subfldr
   For Each subfldr In folder.Packages
      If Not subfldr.IsShortcut Then
         checkJidxNamespace = checkJidxNamespace(subfldr, obj)
         if (checkJidxNamespace = false) then
            Exit Function
          end if
      end if
    next
    
end function

function checkJidx(obj, tabl)
   dim i
   for each i in tabl.Indexes
      if i.Code = obj.Code then
         checkJidx = False
         Exit Function
      end if
   Next
   checkJidx = True
end function</a:CheckGlobalScript>
