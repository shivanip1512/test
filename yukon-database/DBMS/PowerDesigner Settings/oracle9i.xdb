<a:TextTargetItem.Value>{OWNER, VIEW, VIEWSTYLE}

select
   v.owner,
   v.view_name,
   &#39;&#39;
from
   sys.all_views v
[where v.owner=%.q:SCHEMA%]
union select
   v.owner,
