<o:TargetCategory Id="o162">
<a:Name>DB Package Cursor</a:Name>
<a:Code>PDMPKCU</a:Code>
<a:Comment>Manages the database package cursor</a:Comment>
<c:Categories>
<o:TextTargetItem Id="o163">
<a:Name>Add</a:Name>
