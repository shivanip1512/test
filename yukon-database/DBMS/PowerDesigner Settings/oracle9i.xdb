<a:Name>SqlAttrQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, VIEW, SCRIPT EX, ExtObjViewType, ExtObjOIDList, ExtObjSuperView, COMMENT}

select
   v.owner,
   v.view_name,
   &#39;%SqlGetViewText.&#39;||v.owner||v.view_name||&#39;%&#39;,
   v.view_type,
   v.oid_text,
   v.superview_name,
   r.comments
from
   sys.all_views v,
   sys.all_tab_comments r
where  r.table_name = v.view_name
   and r.owner = v.owner
   and r.table_type = &#39;VIEW&#39;
[  and v.owner=%.q:OWNER%]
[  and v.view_name=%.q:VIEW%]
union select
   v.owner,
   v.mview_name,
   &#39;%SqlGetSnapshotText.&#39;||v.owner||v.mview_name||&#39;%&#39;,
   &#39;&#39;, &#39;&#39;, &#39;&#39;,
   r.comments
from
   sys.all_mviews v,
   sys.all_tab_comments r
where  r.table_name = v.mview_name
   and r.owner = v.owner
   and r.table_type = &#39;TABLE&#39;
[  and v.owner=%.q:OWNER%]
[  and v.mview_name=%.q:VIEW%]
order by 1,2
