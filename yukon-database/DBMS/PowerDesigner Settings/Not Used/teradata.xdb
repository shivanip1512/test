<o:TextTargetItem Id="o147">
<a:Name>Create</a:Name>
<a:TextTargetItem.Value>create user %USER%[ from %DBOwner%]
   as [.O:[permanent][perm]]=%PermanentSpace%[%R%? bytes][
   [,] password=[%PASSWORD%?%PASSWORD%:NULL]][.Z:[
   [,] startup=%Startup%][
   [,] temporary=%TemporarySpace%[ bytes]][
   [,] spool=%SpoolSpace%[ bytes]][
   [,] default database=%DefaultDatabase%][
   [,] collation=%Collation%][
   [,] account=%Account%][
   [,] %Fallback%][
   [,] %Journal%][
   [,] %AfterJournal%][
   [,] default journal table=%DefaultJournalTable%][
   [,] timezone=%TimeZone%][
   [,] dateform=%DateForm%][
   [,] default character set=%DefaultCharacterSet%][
   [,] default role=%DefaultRole%][
   [,] profile=%Profile%]]</a:TextTargetItem.Value>
<a:Comment>Command for creating a user</a:Comment>
