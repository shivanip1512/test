<a:Name>Database</a:Name>
<a:Code>DTBS</a:Code>
<a:Comment>The following system variables are available:
   &quot;DATABASE&quot;     // generated code of the database
</a:Comment>
<c:Categories>
<o:BooleanTargetItem Id="o135">
<a:Name>Enable</a:Name>
<a:Comment>Database allowed</a:Comment>
<a:Value>1</a:Value>
</o:BooleanTargetItem>
<o:TextTargetItem Id="o136">
<a:Name>Create</a:Name>
<a:TextTargetItem.Value>create database %DATABASE% [FROM %FromDatabaseName%] 
as [.O:[permanent][perm]]=%PermanentSpace%[%R%? bytes][.Z:[
   [,] spool=%SpoolSpace%[ bytes]][
   [,] temporary=%TemporarySpace%[ bytes]][
   [,] account=%Account%][
   [,] %Fallback%][
   [,] %Journal%][
   [,] %AfterJournal%][
   [,] default journal table=%DefaultJournalTable%]]
</a:TextTargetItem.Value>
<a:Comment>Command for creating a database. Example: create database %DATABASE%</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o137">
<a:Name>Options</a:Name>
<a:Comment>Available options for creating a database</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o138">
<a:Name>Drop</a:Name>
<a:TextTargetItem.Value>drop database %DATABASE% </a:TextTargetItem.Value>
<a:Comment>Command for dropping a database. Example: drop database %DATABASE%</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o139">
<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{DATABASE, PermanentSpace, SpoolSpace, TemporarySpace, Account,Fallback,Journal, AfterJournal}

select 
   d.DatabaseName,
   d.PermSpace,
   d.SpoolSpace,
   d.TempSpace,
   d.AccountName,
   case d.ProtectionType
       when &#39;F&#39; then &#39;FALLBACK&#39;
       when &#39;N&#39; then &#39;NONE&#39;
       else &#39;PROTECTION&#39;
   end,
   case SUBSTR(d.JournalFlag,1,1)
       when &#39;N&#39; then (case SUBSTR(d.JournalFlag,2,1) when &#39;N&#39; then &#39;NO JOURNAL&#39; else &#39;&#39; end)
       when &#39;S&#39; then (case SUBSTR(d.JournalFlag,2,1) when &#39;S&#39; then &#39;JOURNAL&#39;  else &#39;BEFORE JOURNAL&#39; end)
       when &#39;D&#39; then (case SUBSTR(d.JournalFlag,2,1) when &#39;D&#39; then &#39;DUAL JOURNAL&#39; else &#39;&#39; end)
       else &#39;&#39;
   end,	
   case SUBSTR(d.JournalFlag,2,1)
       when &#39;N&#39; then (case SUBSTR(d.JournalFlag,1,1) when &#39;N&#39; then &#39;&#39; else &#39;NO AFTER JOURNAL&#39; end)
       when &#39;S&#39; then (case SUBSTR(d.JournalFlag,1,1) when &#39;S&#39; then &#39;&#39; else &#39;AFTER JOURNAL&#39; end)
       when &#39;D&#39; then (case SUBSTR(d.JournalFlag,1,1) when &#39;D&#39; then &#39;&#39; else &#39;DUAL AFTER JOURNAL&#39; end)
       when &#39;L&#39; then &#39;LOCAL AFTER JOURNAL&#39;
       else &#39;&#39;
   end
from DBC.Databases d
where 
   d.DatabaseName != &#39;PUBLIC&#39;
   and d.DBKind = &#39;D&#39;
[  and d.OwnerName=%.q:CATALOG%]</a:TextTargetItem.Value>
<a:Comment>SQL query to list objects</a:Comment>
</o:TextTargetItem>
