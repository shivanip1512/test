<o:TextTargetItem Id="o96">
<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, TABLE, COLUMN, COLNNAME, DATATYPE, NOTNULL, IDENTITY, ExtGenAlways, DEFAULT, COMMENT, FORMAT, CharacterSet, Compress}

select
   c.CreatorName,
   c.TableName,
   c.ColumnName,
   c.ColumnTitle,
   case (c.ColumnType)
      when &#39;CF&#39; then &#39;char(&#39; || trim(case (c.CharType) when 2 then (c.ColumnLength/2) when 4 then (c.ColumnLength/2) else (c.ColumnLength) end) || &#39;)&#39;
      when &#39;CV&#39; then &#39;varchar(&#39; || trim(case (c.CharType) when 2 then (c.ColumnLength/2) when 4 then (c.ColumnLength/2) else (c.ColumnLength) end) || &#39;)&#39;
      when &#39;D&#39;  then &#39;decimal(&#39; || trim(c.DecimalTotalDigits) || &#39;,&#39; || trim(c.DecimalFractionalDigits) || &#39;)&#39;
      when &#39;I&#39;  then &#39;integer&#39;
      when &#39;I1&#39; then &#39;byteint&#39;
      when &#39;I2&#39; then &#39;smallint&#39;
      when &#39;F&#39;  then &#39;float&#39;
      when &#39;DA&#39; then &#39;date&#39;
      when &#39;AT&#39; then &#39;time&#39;
      when &#39;TS&#39; then &#39;timestamp&#39;
      when &#39;TZ&#39; then &#39;time with time zone&#39;
      when &#39;SZ&#39; then &#39;timestamp with time zone&#39;
      when &#39;YR&#39; then &#39;interval year&#39;
      when &#39;YM&#39; then &#39;interval year to month&#39;
      when &#39;MO&#39; then &#39;interval month&#39;
      when &#39;DY&#39; then &#39;interval day&#39;
      when &#39;DH&#39; then &#39;interval day to hour&#39;
      when &#39;DM&#39; then &#39;interval day to minute&#39;
      when &#39;DS&#39; then &#39;interval day to second&#39;
      when &#39;HR&#39; then &#39;interval hour&#39;
      when &#39;HM&#39; then &#39;interval hour to minute&#39;
      when &#39;MI&#39; then &#39;interval minute&#39;
      when &#39;BF&#39; then &#39;byte&#39;
      when &#39;BV&#39; then &#39;byte varying&#39;
      else c.ColumnType
   end,
   case when (c.Nullable = &#39;N&#39;) then &#39;NOT NULL&#39; else &#39;NULL&#39; end,
   case(c.idColType) when &#39;GD&#39; then &#39;identity&#39; when &#39;GA&#39; then &#39;identity&#39; else &#39;&#39; end,
   case(c.idColType) when &#39;GD&#39; then &#39;false&#39; else &#39;true&#39; end,
   case when (
         (length(c.defaultvalue) &gt; 2) and 
         (substring(c.ColumnType from 1 for 1) = &#39;C&#39;) and
         (trim(substring(c.defaultvalue from 2 for length(c.defaultvalue) - 2)) = &#39;&#39;)) then &#39;~&#39; || c.defaultvalue || &#39;~&#39;
        else c.defaultvalue
   end,
   c.CommentString,
   c.ColumnFormat,
   case (c.CharType)
      when 1 then &#39;latin&#39;
      when 2 then &#39;unicode&#39;
      when 3 then &#39;kanjisjis&#39;
      when 4 then &#39;graphic&#39;
      when 5 then &#39;kanji1&#39;
      else &#39;&#39;
   end,
   c.CompressValueList
from
   DBC.Columns c
where 1=1
[  and c.TableName = %.q:TABLE%]
[  and c.CreatorName = %.q:OWNER%]
[  and c.DatabaseName = %.q:CATALOG%]
order by
   1, 2, c.ColumnId</a:TextTargetItem.Value>
<a:Comment>SQL query to list objects</a:Comment>
</o:TextTargetItem>
