<o:TextTargetItem Id="o113">
<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{COWNER ID, CHILD ID, FKCONSTRAINT ID, POWNER ID, PARENT ID, CKEYCOLUMNS ..., FKEYCOLUMNS ...}

select
   c.CreatorName,
   c.TableName,
   f.IndexName,
   p.CreatorName,
   p.TableName,
   f.ParentKeyColumn || &#39;,&#39;,
   f.ChildKeyColumn || &#39;,&#39;
from
   dbc.All_RI_Children f
      join dbc.Tables c on (c.DatabaseName = f.ChildDB and c.TableName = f.ChildTable and c.TableKind = &#39;T&#39;)
      join dbc.Tables p on (p.DatabaseName = f.ParentDB and p.TableName = f.ParentTable and p.TableKind = &#39;T&#39;)
where
   c.TableName not in (select i.TableName from dbc.indices i where i.databasename=c.databasename and i.CreateTimeStamp is null)
[  and c.DatabaseName = %.q:CATALOG%]
[  and c.CreatorName = %.q:OWNER%]
[  and c.TableName = %.q:TABLE%]
order by 1,2,3,4,5
</a:TextTargetItem.Value>
<a:Comment>SQL query to list objects</a:Comment>
</o:TextTargetItem>
