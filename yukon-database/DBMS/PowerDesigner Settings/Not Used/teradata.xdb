<a:Comment>Maximum object code length</a:Comment>
<a:IntegerTargetItem.Value>128</a:IntegerTargetItem.Value>
</o:IntegerTargetItem>
<o:TextTargetItem Id="o151">
<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{USER, PermanentSpace, SpoolSpace, TemporarySpace, Account,Fallback,Journal, AfterJournal}

select
   d.DatabaseName,
   d.PermSpace,
   d.SpoolSpace,
   d.TempSpace,
   d.AccountName,
   case d.ProtectionType
       when &#39;F&#39; then &#39;FALLBACK&#39;
       when &#39;N&#39; then &#39;NONE&#39;
       else &#39;PROTECTION&#39;
   end,
   case SUBSTR(d.JournalFlag,1,1)
       when &#39;N&#39; then (case SUBSTR(d.JournalFlag,2,1) when &#39;N&#39; then &#39;NO JOURNAL&#39; else &#39;&#39; end)
       when &#39;S&#39; then (case SUBSTR(d.JournalFlag,2,1) when &#39;S&#39; then &#39;JOURNAL&#39;  else &#39;BEFORE JOURNAL&#39; end)
       when &#39;D&#39; then (case SUBSTR(d.JournalFlag,2,1) when &#39;D&#39; then &#39;DUAL JOURNAL&#39; else &#39;&#39; end)
       else &#39;&#39;
   end,	
   case SUBSTR(d.JournalFlag,2,1)
       when &#39;N&#39; then (case SUBSTR(d.JournalFlag,1,1) when &#39;N&#39; then &#39;&#39; else &#39;NO AFTER JOURNAL&#39; end)
       when &#39;S&#39; then (case SUBSTR(d.JournalFlag,1,1) when &#39;S&#39; then &#39;&#39; else &#39;AFTER JOURNAL&#39; end)
       when &#39;D&#39; then (case SUBSTR(d.JournalFlag,1,1) when &#39;D&#39; then &#39;&#39; else &#39;DUAL AFTER JOURNAL&#39; end)
       when &#39;L&#39; then &#39;LOCAL AFTER JOURNAL&#39;
       else &#39;&#39;
   end
from
   DBC.Databases d
where
   d.DatabaseName != &#39;PUBLIC&#39;
   and d.DBKind = &#39;U&#39;
[  and d.OwnerName = %.q:CATALOG%]
[ and d.DatabaseName = %.q:SCHEMA%]
</a:TextTargetItem.Value>
<a:Comment>SQL query to list objects</a:Comment>
</o:TextTargetItem>
