<o:TextTargetItem Id="o153">
<a:Name>SqlAttrQuery</a:Name>
<a:TextTargetItem.Value>{USER, Startup, Collation, DefaultDatabase, DefaultJournalTable, DateForm, DefaultCharacterSet, DefaultRole,Profile}

select 
   u.UserName,
   u.StartupString,
   case u.DefaultCollation
      when &#39;A&#39; then &#39;ASCII&#39;
      when &#39;E&#39; then &#39;EBCDIC&#39;
      when &#39;M&#39; then &#39;MULTINATIONAL&#39;
      when &#39;H&#39; then &#39;HOST&#39;
      else &#39;&#39;
   end,
   u.DefaultDataBase,
   coalesce(trim(j.Journal_DB) || &#39;.&#39; || trim(j.JournalName), trim(j.JournalName), &#39;&#39;),
   u.DefaultDateForm,
   case u.DefaultCharType
      when 1 then &#39;Latin&#39;
      when 2 then &#39;Unicode&#39;
      when 3 then &#39;KanjiSJIS&#39;
      when 4 then &#39;Graphic&#39;
      when 5 then &#39;Kanji1&#39;
      else &#39;&#39;
   end,
   u.RoleName,
   u.ProfileName
from 
   DBC.Users u 
   left outer join DBC.User_Default_Journals j on (u.UserName = j.UserName)
where 1=1
[  and u.OwnerName = %.q:CATALOG%]
[  and u.UserName = %.q:USER%]
</a:TextTargetItem.Value>
<a:Comment>SQL query to reverse object attributes</a:Comment>
</o:TextTargetItem>
</c:Categories>
</o:TargetCategory>
<o:TargetCategory Id="o154">
<a:Name>View</a:Name>
<a:Code>VIEW</a:Code>
<a:Comment>The following system variables are available:
