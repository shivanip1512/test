<a:TextTargetItem.Value>alter table %TABLE%
    add constraint %FKCONSTRAINT% foreign key (%FKEYCOLUMNS%)
       references [%R%?[with [no ]check option ]]%PARENT%[ (%CKEYCOLUMNS%)]</a:TextTargetItem.Value>
