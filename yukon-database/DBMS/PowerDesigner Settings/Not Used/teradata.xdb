<o:TextTargetItem Id="o79">
<a:Name>SqlAttrQuery</a:Name>
<a:TextTargetItem.Value>{OWNER ID, TABLE ID, INDEX ID, OrderByColumnList ...}

select
   j.CreatorName,
   j.TableName,
   j.IndexName,
   i.ColumnName ||&#39;,&#39;
from
   dbc.Indices i,
   dbc.Indices j
where
   i.IndexType in (&#39;I&#39;)
   and j.IndexType in (&#39;V&#39;, &#39;O&#39;)
   and j.ColumnPosition = 1
   and i.CreatorName = j.CreatorName
   and i.TableName = j.TableName
   and i.IndexNumber = j.IndexNumber + 4
[  and j.DatabaseName = %.q:CATALOG%]
[  and i.CreatorName = %.q:SCHEMA%]
[  and j.TableName = %.q:TABLE%]
[  and j.IndexName = %.q:INDEX%]
order by 1, 2, 3, i.ColumnPosition
</a:TextTargetItem.Value>
