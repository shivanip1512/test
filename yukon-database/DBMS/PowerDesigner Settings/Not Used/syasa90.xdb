<a:Value>1</a:Value>
</o:BooleanTargetItem>
<o:TextTargetItem Id="o106">
<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{USER}

select 
   u.user_name 
from 
   sys.sysuserperms u
where
  u.user_group = &#39;N&#39;
[and UPPER(u.user_name) = %.qU:SCHEMA%]
