<o:TextTargetItem Id="o142">
<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{OWNER ID, TABLE ID, ISPKEY ID, CONSTNAME ID, ClusteredKey ID, COLUMNS ...}

select
   u.user_name,
   t.table_name,
   1,
   isnull(h.constraint_name, string(t.table_name, &#39;_PK&#39;)),
   ifnull(k.attribute_name, &#39;&#39;, &#39;TRUE&#39;),
   string(c.column_name, &#39;,&#39;),
   c.column_id
from 
   sys.sysuserperms u
      join sys.systable t on (t.creator=u.user_id)
      join sys.syscolumn c on (c.table_id=t.table_id)
      left outer join sys.sysconstraint h on (h.table_id = t.table_id and h.constraint_type = &#39;P&#39; and h.index_id is null)
      left join sysattribute a on (t.table_id = a.object_id and a.object_type = &#39;T&#39; and a.attribute_value = 0)
      left join sysattributename k on (a.attribute_id = k.attribute_id and k.attribute_name = &#39;Clustered index&#39;)
where 
   t.primary_root != 0
   and c.pkey=&#39;Y&#39;
[  and t.table_name=%.q:TABLE%]
[  and u.user_name=%.q:OWNER%]
union select
   u.user_name,
   t.table_name,
   0,
   isnull(h.constraint_name, string(t.table_name, &#39;_K&#39;, i.index_id)),
   ifnull(k.attribute_name, &#39;&#39;, &#39;TRUE&#39;),
   string(c.column_name, &#39;,&#39;),
   x.sequence
from
   sys.sysuserperms u
      join sys.systable t on (t.creator=u.user_id)
      join sys.sysindex i on (i.table_id=t.table_id)
      join sys.sysixcol x on (x.index_id=i.index_id and x.table_id=i.table_id)
      join sys.syscolumn c on (c.column_id=x.column_id and c.table_id=x.table_id)
      left outer join sys.sysconstraint h on (h.table_id = t.table_id and h.index_id = i.index_id)
      left join sysattribute a on (t.table_id = a.object_id and a.object_type = &#39;T&#39; and a.attribute_value = i.index_id)
      left join sysattributename k on (a.attribute_id = k.attribute_id and k.attribute_name = &#39;Clustered index&#39;)
where 
   i.&quot;unique&quot; = &#39;U&#39;
[   and t.table_name=%.q:TABLE%]
[   and u.user_name=%.q:OWNER%]
order by
   1, 2, 3 desc, 7
</a:TextTargetItem.Value>
<a:Comment>SQL query to list objects</a:Comment>
</o:TextTargetItem>
</c:Categories>
