<a:TextTargetItem.Value>[%WEBSERVICETYPE%=DISH?if exists(select 1 from sys.syswebservice where service_name = %.q:WEBSERVICE%) then
   drop service %WEBSERVICE%
end if]</a:TextTargetItem.Value>
<a:Comment>Command for dropping a web service. Example: drop service %SERVICE%</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o230">
<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{ WEBSERVICE, WEBSERVICETYPE, WEBSERVICEPATH, Prefix, COMMENT, WEBAUTHREQUIRED, WEBCNCTSECURED, WEBUSER }

select 
if (locate(service_name, &#39;/&#39;) &gt; 1) then substr(service_name, locate(service_name, &#39;/&#39;, -1)+1) else service_name endif as name,
&#39;DISH&#39; as type, 
w.service_name as localpath, 
w.parameter as service_prefix, w.remarks, w.auth_required, w.secure_required, u.user_name
from syswebservice w
left outer join sysuserperms u on ( w.user_id = u.user_id )
where w.service_type=&#39;DISH&#39;
UNION
select distinct 
if (locate(service_name, &#39;/&#39;) &gt; 1) then 
  if (locate(substr(service_name, 1, locate(service_name, &#39;/&#39;, -1)-1), &#39;/&#39;) &gt; 1)
  then substr(service_name, locate(substr(service_name, 1, locate(service_name, &#39;/&#39;, -1)-1), &#39;/&#39;)+1, locate(service_name, &#39;/&#39;,-1)- locate(substr(service_name, 1, locate(service_name, &#39;/&#39;, -1)-1), &#39;/&#39;) - 1) 
  else substr(service_name, 1, locate(service_name, &#39;/&#39;, -1)-1) 
  endif
else ucase(service_type) endif, 
service_type,
if (locate(service_name, &#39;/&#39;) &gt; 1) then substr(service_name, 1, locate(service_name, &#39;/&#39;, -1)-1) else &#39;/&#39; endif, 
NULL,NULL, NULL, NULL, NULL
from syswebservice w
where service_type&lt;&gt;&#39;DISH&#39; and service_type&lt;&gt;&#39;SOAP&#39;</a:TextTargetItem.Value>
<a:Comment>SQL query to list objects</a:Comment>
</o:TextTargetItem>
