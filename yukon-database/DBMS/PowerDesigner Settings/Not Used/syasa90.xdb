   p.proc_name,
   isnull(p.source, p.proc_defn),
   p.remarks
from
   sys.sysprocedure p
where 1=1
[  and user_name(p.creator) = %.q:SCHEMA%]
[  and p.proc_name = %.q:PROC%]
