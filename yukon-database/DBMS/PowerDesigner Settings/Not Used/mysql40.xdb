<o:TextTargetItem Id="o56">
<a:Name>AfterCreate</a:Name>
<a:TextTargetItem.Value>.foreach_item(OutReferences)
   .ifnot (%hasFKIndex%)
/*==============================================================*/
/* Index: &quot;%referenceCode%_FK&quot;                                            */
/*==============================================================*/
create index %referenceCode%_FK
(
   %fkColumns%
);

   .endif
.next</a:TextTargetItem.Value>
<a:Comment>Commands executed after create statement</a:Comment>
</o:TextTargetItem>
