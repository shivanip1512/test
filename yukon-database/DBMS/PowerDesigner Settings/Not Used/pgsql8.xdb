<o:TextTargetItem Id="o115">
<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{POWNER, PARENT, COWNER, CHILD, CONSTNAME, Deferrable, ForeignKeyConstraintDeferred, UPDCONST, DELCONST, FKEYCOLUMNS, CKEYCOLUMNS}

select
   pg_get_userbyid(p.relowner),
   p.relname,
   pg_get_userbyid(c.relowner),
   c.relname,
   k.conname,
   case when (k.condeferrable) then &#39;true&#39; else &#39;false&#39; end,
   case when (k.condeferred) then &#39;true&#39; else &#39;false&#39; end,
   case(k.confupdtype) when &#39;r&#39; then &#39;restrict&#39; when &#39;c&#39; then &#39;cascade&#39; when &#39;n&#39; then &#39;set null&#39; when &#39;d&#39; then &#39;set default&#39; else &#39;no action&#39; end::varchar,
   case(k.confdeltype) when &#39;r&#39; then &#39;restrict&#39; when &#39;c&#39; then &#39;cascade&#39; when &#39;n&#39; then &#39;set null&#39; when &#39;d&#39; then &#39;set default&#39; else &#39;no action&#39; end::varchar,
   substring(substring(pg_get_constraintdef(k.oid) from &#39;FOREIGN KEY (.*) REFERENCES&#39;) from 2 for char_length(substring(pg_get_constraintdef(k.oid) from &#39;FOREIGN KEY (.*) REFERENCES&#39;)) - 2),
   substring(substring(pg_get_constraintdef(k.oid) from &#39;REFERENCES &#39;||p.relname||&#39;(.*)&#39;) from 2 for position(&#39;)&#39; in substring(pg_get_constraintdef(k.oid) from &#39;REFERENCES &#39;||p.relname||&#39;(.*)&#39;) ) - 2)
from 
   pg_constraint k
   join pg_class p on (p.oid = k.confrelid)
   join pg_class c on (c.oid = k.conrelid)
where 
   k.contype = &#39;f&#39;
[  and pg_get_userbyid(c.relowner)=%.q:OWNER%]
[  and c.relname=%.q:TABLE%]
order by 1, 2, 3, 4, 5</a:TextTargetItem.Value>
<a:Comment>SQL query to list objects</a:Comment>
</o:TextTargetItem>
</c:Categories>
