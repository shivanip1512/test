<o:TextTargetItem Id="o88">
<a:Name>Create</a:Name>
<a:TextTargetItem.Value>create [%UNIQUE% ]index [%QUALIFIER%]%INDEX% on [%TABLQUALIFIER%]%TABLE% (
%CIDXLIST%
)
[%OPTIONS%]</a:TextTargetItem.Value>
<a:Comment>Command for creating an index. Example: create index %INDEX%</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o89">
<a:Name>AddColIndex</a:Name>
<a:TextTargetItem.Value>   %20:COLUMN% [%ASC%]</a:TextTargetItem.Value>
<a:Comment>Command for defining an index column</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o90">
<a:Name>Options</a:Name>
<a:TextTargetItem.Value>specification only
include : composite=yes
{
&lt;column_list&gt; : composite=yes, separator=yes, parenthesis=yes
{
&lt;column&gt; %s : multiple=yes, squoted=no
}
}
cluster
extend using %s : composite=yes, parenthesis=yes, separator=yes
{
&lt;expression&gt; %s : multiple=yes
}
pctfree %d : default=10
minpctused %d
&lt;reverse_scans&gt; %s : list=disallow reverse scans|allow reverse scans
collect %s : list=statistics|detailed statistics|detailed sampled statistics, default=statistics</a:TextTargetItem.Value>
<a:Comment>Default options for creating an index</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o91">
<a:Name>DefOptions</a:Name>
<a:Comment>Default values for index options</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o92">
<a:Name>SqlSysIndexQuery</a:Name>
<a:TextTargetItem.Value>{OWNER,TABLE,INDEX}

select 
   tbcreator,
   tbname,
   name
from 
   sysibm.sysindexes
where
   creator=&#39;SYSIBM&#39;
   and tbname=%.q:TABLE%
   and tbcreator=%.q:OWNER%
</a:TextTargetItem.Value>
<a:Comment>SQL query to list system indexes created by the database</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o93">
<a:Name>Drop</a:Name>
<a:TextTargetItem.Value>drop index [%QUALIFIER%]%INDEX%</a:TextTargetItem.Value>
<a:Comment>Command for dropping an index. Example: drop index %INDEX%</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o94">
<a:Name>SqlIdxIncludeCols</a:Name>
<a:TextTargetItem.Value>{VAR ID, VAL ...}

select
   c.indschema concat c.indname,
   c.colname concat case c.colseq when (select max(x.colseq) from sysibm.sysindexcoluse x where x.indschema = c.indschema and x.indname = c.indname and x.colorder = &#39;I&#39;) then &#39;&#39; else &#39;,&#39; end
from
   sysibm.sysindexcoluse c
where
   c.colorder = &#39;I&#39;
[  and c.indschema = %.q:OWNER%]
[  and c.indname = %.q:INDEX%]
order by 1, c.colseq
</a:TextTargetItem.Value>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
</o:TextTargetItem>
<o:TextTargetItem Id="o95">
<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{TABLOWNER ID, TABLE ID, OWNER ID, INDEX ID, UNIQUE ID, CIDXLIST ...}

select
   i.tbcreator,
   i.tbname,
   i.creator,
   i.name,
   case(i.uniquerule) when &#39;U&#39; then &#39;unique&#39; else &#39;&#39; end,
   c.colname || case(c.colorder) when &#39;D&#39; then &#39; desc&#39; else &#39;&#39; end || &#39;,&#39;
from 
   sysibm.sysindexes i
   join sysibm.sysindexcoluse c on (c.indschema=i.creator and c.indname=i.name)
where 
   i.user_defined &lt;&gt; 0
   and c.colorder in (&#39;A&#39;, &#39;D&#39;)
[  and i.tbcreator = %.q:SCHEMA%]
[  and i.tbname = %.q:TABLE%]
order by 1, 2, 3, 4, c.colseq
</a:TextTargetItem.Value>
<a:Comment>SQL query to list objects</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o96">
<a:Name>SqlOptsQuery</a:Name>
<a:TextTargetItem.Value>{TABLOWNER, TABLE, OWNER, INDEX, OPTIONS EX}

select
   i.tbcreator,
   i.tbname,
   i.creator,
   i.name,
   case when (i.unique_colcount &lt; i.colcount and i.unique_colcount &lt;&gt; -1) then &#39;include (%SqlIdxIncludeCols.&#39; concat i.creator concat i.name concat &#39;%)&#39; else &#39;&#39; end
   concat case i.indextype when &#39;CLUS&#39; then &#39; cluster&#39; else &#39;&#39; end
   concat case i.pctfree when -1 then &#39;&#39; else &#39; pctfree &#39; concat char(i.pctfree) end
   concat case i.minpctused when 0 then &#39;&#39; else &#39; minpctused &#39; concat char(i.minpctused) end
   concat case i.reverse_scans when &#39;Y&#39; then &#39; allow reverse scans&#39; when &#39;N&#39; then &#39; disallow reverse scans&#39; else &#39;&#39; end
from 
   sysibm.sysindexes i
where 
   i.user_defined &lt;&gt; 0
[  and i.tbcreator = %.q:SCHEMA%]
[  and i.tbname = %.q:TABLE%]</a:TextTargetItem.Value>
<a:Comment>SQL query to reverse object physical options</a:Comment>
</o:TextTargetItem>
