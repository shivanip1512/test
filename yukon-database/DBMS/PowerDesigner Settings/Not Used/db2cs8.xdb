<o:TextTargetItem Id="o113">
<a:Name>Options</a:Name>
<a:TextTargetItem.Value>&lt;datalink_option&gt; : composite=yes
{
linktype url
no link control
file link control : composite=yes
{
mode db2options
integrity %s : default=all
read permission %s : list=fs|db, default=fs
write permission %s : list=fs|blocked|admin requiring token for update|admin not requiring token for update, default=fs
recovery %s : list=yes|no, default=no
on unlink %s : list=restore|delete, default=restore
}
}
inline length %d
compress system default
&lt;identity_options&gt; : composite=yes, parenthesis=yes, separator=yes
{
start with %d : default=1
increment by %d : default=1
&lt;minvalue&gt; : composite=yes
{
minvalue %d
no minvalue
}
&lt;maxvalue&gt; : composite=yes
{
maxvalue %d
no maxvalue
}
&lt;cycle&gt; %s : default=no cycle, list=cycle|no cycle
&lt;cache&gt; : composite=yes
{
cache %d : default=20
no cache
}
&lt;order&gt; %s : default=no order, list=order|no order
}</a:TextTargetItem.Value>
