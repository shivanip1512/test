   update [%CQUALIFIER%]%CHILD%
    set   .JOIN(&quot;[%CQUALIFIER%]%CHILD%.%FK% = new_upd.%PK%&quot;, &quot;&quot;, &quot;,&quot;)
    where  (.JOIN(&quot;[%CQUALIFIER%]%CHILD%.%FK% = old_upd.%PK%&quot;, &quot;and   &quot;, &quot;&quot;, &quot;) and&quot;)
