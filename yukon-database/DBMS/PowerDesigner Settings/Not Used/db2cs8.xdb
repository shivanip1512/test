<o:TextTargetItem Id="o257">
<a:Name>Create</a:Name>
<a:TextTargetItem.Value>create sequence [%QUALIFIER%]%SQNC%
[%OPTIONS%]</a:TextTargetItem.Value>
<a:Comment>Command to create a sequence</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o258">
<a:Name>Drop</a:Name>
<a:TextTargetItem.Value>drop sequence [%QUALIFIER%]%SQNC%</a:TextTargetItem.Value>
<a:Comment>Command to drop a sequence</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o259">
<a:Name>Options</a:Name>
<a:TextTargetItem.Value>as %s : default=integer
&lt;initial&gt; : composite=yes
{
increment by %d
start with %d
}
&lt;limits&gt; : composite=yes
{
maxvalue %d
nomaxvalue
minvalue %d
nominvalue
}
&lt;cycle&gt; %s : default=no cycle, list=cycle|no cycle
&lt;cache&gt; : composite=yes
{
cache %d : default=20
no cache
}
&lt;order&gt; %s : default=no order, list=order|no order</a:TextTargetItem.Value>
<a:Comment>Options for creating a sequence</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o260">
<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, SQNC, OPTIONS}

select
   s.seqschema,
   s.seqname,
   &#39; increment by &#39; concat rtrim(char(int(s.increment)))
   concat &#39; start with &#39; concat rtrim(char(int(s.start)))
   concat case s.maxvalue when 2147483647 then &#39; no maxvalue&#39; else &#39; maxvalue &#39; concat rtrim(char(int(s.maxvalue))) end
   concat &#39; minvalue &#39; concat rtrim(char(int(s.minvalue)))
   concat case s.cycle when &#39;N&#39; then &#39; nocycle&#39; else &#39; cycle&#39; end
   concat case s.cache when 0 then &#39; nocache&#39; else &#39; cache &#39; concat rtrim(char(int(s.cache))) end
   concat case s.order when &#39;N&#39; then &#39; noorder&#39; else &#39; order&#39; end
from
   sysibm.syssequences s
order by 1, 2</a:TextTargetItem.Value>
<a:Comment>SQL query to list objects</a:Comment>
</o:TextTargetItem>
