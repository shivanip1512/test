<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<points xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../points.xsd">
    <!--- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -Analog- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
    <point offset="1" type="Analog">
        <name>Blink Count</name>
        <multiplier value="1"/>
        <unitofmeasure value="COUNTS"/>
        <decimalplaces value="0"/>
    </point>
    <point offset="10" type="Analog">
        <name>Relay 1 Run Time Data Log</name>
        <multiplier value="1"/>
        <unitofmeasure value="MINUTES"/>
        <decimalplaces value="0"/>
    </point>
    <point offset="11" type="Analog">
        <name>Relay 1 Shed Time Data Log</name>
        <multiplier value="1"/>
        <unitofmeasure value="MINUTES"/>
        <decimalplaces value="0"/>
    </point>
    <point offset="12" type="Analog">
        <name>Relay 1 kW Load Size</name>
        <multiplier value="1"/>
        <unitofmeasure value="KW"/>
        <decimalplaces value="4"/>
    </point>
    <point offset="13" type="Analog">
        <name>Relay 1 Remaining Control Time</name>
        <multiplier value="1"/>
        <unitofmeasure value="SECONDS"/>
        <decimalplaces value="0"/>
    </point>
    <point offset="15" type="Analog">
        <name>Reporting Interval</name>
        <multiplier value="1"/>
        <unitofmeasure value="MINUTES"/>
        <decimalplaces value="0"/>
    </point>
    <point offset="16" type="Analog">
        <name>Recording Interval</name>
        <multiplier value="1"/>
        <unitofmeasure value="MINUTES"/>
        <decimalplaces value="0"/>
    </point>
    <point offset="17" type="Analog">
        <name>Total LUF Event Count</name>
        <multiplier value="1"/>
        <unitofmeasure value="COUNTS"/>
        <decimalplaces value="0"/>
    </point>
    <point offset="18" type="Analog">
        <name>Total LUV Event Count</name>
        <multiplier value="1"/>
        <unitofmeasure value="COUNTS"/>
        <decimalplaces value="0"/>
    </point>
    <point offset="20" type="Analog">
        <name>Relay 2 Run Time Data Log</name>
        <multiplier value="1"/>
        <unitofmeasure value="MINUTES"/>
        <decimalplaces value="0"/>
    </point>
    <point offset="21" type="Analog">
        <name>Relay 2 Shed Time Data Log</name>
        <multiplier value="1"/>
        <unitofmeasure value="MINUTES"/>
        <decimalplaces value="0"/>
    </point>
    <point offset="22" type="Analog">
        <name>Relay 2 kW Load Size</name>
        <multiplier value="1"/>
        <unitofmeasure value="KW"/>
        <decimalplaces value="4"/>
    </point>
    <point offset="23" type="Analog">
        <name>Relay 2 Remaining Control Time</name>
        <multiplier value="1"/>
        <unitofmeasure value="SECONDS"/>
        <decimalplaces value="0"/>
    </point>
    <point offset="24" type="Analog">
        <name>Relay 3 Run Time Data Log</name>
        <multiplier value="1"/>
        <unitofmeasure value="MINUTES"/>
        <decimalplaces value="0"/>
    </point>
    <point offset="25" type="Analog">
        <name>Relay 3 Shed Time Data Log</name>
        <multiplier value="1"/>
        <unitofmeasure value="MINUTES"/>
        <decimalplaces value="0"/>
    </point>
    <point offset="26" type="Analog">
        <name>Relay 3 kW Load Size</name>
        <multiplier value="1"/>
        <unitofmeasure value="KW"/>
        <decimalplaces value="4"/>
    </point>
    <point offset="27" type="Analog">
        <name>Relay 3 Remaining Control Time</name>
        <multiplier value="1"/>
        <unitofmeasure value="SECONDS"/>
        <decimalplaces value="0"/>
    </point>
    <point offset="342" type="Analog">
        <name>Average Voltage</name>
        <multiplier value="1"/>
        <unitofmeasure value="VOLTS"/>
        <decimalplaces value="3"/>
    </point>
    <point offset="355" type="Analog">
        <name>Minimum Voltage</name>
        <multiplier value="1"/>
        <unitofmeasure value="VOLTS"/>
        <decimalplaces value="3"/>
    </point>
    <point offset="356" type="Analog">
        <name>Maximum Voltage</name>
        <multiplier value="1"/>
        <unitofmeasure value="VOLTS"/>
        <decimalplaces value="3"/>
    </point>
    
    <!-- Power Quality Response (GridBallast) Points -->
    <point offset="900" type="Analog">
        <name>LOF Trigger Period</name>
        <multiplier value=".001"/>
        <unitofmeasure value="MS"/>
        <decimalplaces value="3"/>
    </point>
    <point offset="901" type="Analog">
        <name>LOF Restore Period</name>
        <multiplier value=".001"/>
        <unitofmeasure value="MS"/>
        <decimalplaces value="3"/>
    </point>
    <point offset="902" type="Analog">
        <name>LOF Trigger Time</name>
        <multiplier value="1"/>
        <unitofmeasure value="MS"/>
        <decimalplaces value="0"/>
    </point>
    <point offset="903" type="Analog">
        <name>LOF Restore Time</name>
        <multiplier value="1"/>
        <unitofmeasure value="MS"/>
        <decimalplaces value="0"/>
    </point>
    <point offset="904" type="Analog">
        <name>Total LOF Event Count</name>
        <multiplier value="1"/>
        <unitofmeasure value="COUNTS"/>
        <decimalplaces value="0"/>
    </point>
    <point offset="905" type="Analog">
        <name>LOF Start Random Time</name>
        <multiplier value="1"/>
        <unitofmeasure value="MS"/>
        <decimalplaces value="0"/>
    </point>
    <point offset="906" type="Analog">
        <name>LOF End Random Time</name>
        <multiplier value="1"/>
        <unitofmeasure value="MS"/>
        <decimalplaces value="0"/>
    </point>
    <point offset="907" type="Analog">
        <name>LOF Min Event Duration</name>
        <multiplier value="1"/>
        <unitofmeasure value="SECONDS"/>
        <decimalplaces value="0"/>
    </point>
    <point offset="908" type="Analog">
        <name>LOF Max Event Duration</name>
        <multiplier value="1"/>
        <unitofmeasure value="SECONDS"/>
        <decimalplaces value="0"/>
    </point>
    <point offset="909" type="Analog">
        <name>LOV Trigger</name>
        <multiplier value="0.1"/>
        <unitofmeasure value="VOLTS"/>
        <decimalplaces value="1"/>
    </point>
    <point offset="910" type="Analog">
        <name>LOV Restore</name>
        <multiplier value="0.1"/>
        <unitofmeasure value="VOLTS"/>
        <decimalplaces value="1"/>
    </point>
    <point offset="911" type="Analog">
        <name>LOV Trigger Time</name>
        <multiplier value="1"/>
        <unitofmeasure value="MS"/>
        <decimalplaces value="0"/>
    </point>
    <point offset="912" type="Analog">
        <name>LOV Restore Time</name>
        <multiplier value="1"/>
        <unitofmeasure value="MS"/>
        <decimalplaces value="0"/>
    </point>
    <point offset="913" type="Analog">
        <name>Total LOV Event Count</name>
        <multiplier value="1"/>
        <unitofmeasure value="COUNTS"/>
        <decimalplaces value="0"/>
    </point>
    <point offset="914" type="Analog">
        <name>LOV Start Random Time</name>
        <multiplier value="1"/>
        <unitofmeasure value="MS"/>
        <decimalplaces value="0"/>
    </point>
    <point offset="915" type="Analog">
        <name>LOV End Random Time</name>
        <multiplier value="1"/>
        <unitofmeasure value="MS"/>
        <decimalplaces value="0"/>
    </point>
    <point offset="916" type="Analog">
        <name>LOV Min Event Duration</name>
        <multiplier value="1"/>
        <unitofmeasure value="SECONDS"/>
        <decimalplaces value="0"/>
    </point>
    <point offset="917" type="Analog">
        <name>LOV Max Event Duration</name>
        <multiplier value="1"/>
        <unitofmeasure value="SECONDS"/>
        <decimalplaces value="0"/>
    </point>
    <point offset="918" type="Analog">
        <name>Minimum PQR Event Separation</name>
        <multiplier value="1"/>
        <unitofmeasure value="SECONDS"/>
        <decimalplaces value="0"/>
    </point>
    <!-- Warning: GridBallast points have offsets in the 900s. 
         If adding new general-purpose LCR points, add them above GridBallast, with a reasonable offset.-->
         
    <!--- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -Status- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
    <point offset="1" type="Status">
        <name>Control Status</name>
        <stategroup value="TrueFalse"/>
    </point>
    <point offset="2" type="Status">
        <name>Service Status</name>
        <stategroup value="LCR Service Status"/>
    </point>
    <point offset="900" type="Status">
        <name>Power Quality Response Enabled</name>
        <stategroup value="YesNo"/>
    </point>
</points>
