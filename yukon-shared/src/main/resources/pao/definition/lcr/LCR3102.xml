<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<pao xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" paoType="LCR3102" displayGroup="Two Way LCR" displayName="LCR-3102" xsi:noNamespaceSchemaLocation="../pao.xsd">
    <pointFiles>
        <pointFile>LCR3102.xml</pointFile>
    </pointFiles>
    <tags>
        <tag name="ASSET_DETAIL_DISPLAYABLE"/>
        <tag name="COMMANDER_REQUESTS"/>
        <tag name="LOCATE_ROUTE"/>
        <tag name="PORTER_COMMAND_REQUESTS"/>
    </tags>
    <pointInfos>
        <pointInfo name="Comm Status" init="true"/>
        <pointInfo name="CT1 Duty Cycle" init="true"/>
        <pointInfo name="CT2 Duty Cycle" init="true"/>
        <pointInfo name="Last Interval kW Relay 1" init="true"/>
        <pointInfo name="Last Interval kW Relay 2" init="true"/>
        <pointInfo name="Last Interval kW Relay 3" init="true"/>
        <pointInfo name="Last Interval kW Relay 4" init="true"/>
        <pointInfo name="Prop Count" init="true"/>
        <pointInfo name="Relay 1 Remaining Control Time" init="true" attributes="RELAY_1_REMAINING_CONTROL"/>
        <pointInfo name="Relay 1 Shed Time" init="true" attributes="RELAY_1_SHED_TIME_DATA_LOG"/>
        <pointInfo name="Relay 2 Remaining Control Time" init="true" attributes="RELAY_2_REMAINING_CONTROL"/>
        <pointInfo name="Relay 2 Shed Time" init="true" attributes="RELAY_2_SHED_TIME_DATA_LOG"/>
        <pointInfo name="Relay 3 Remaining Control Time" init="true" attributes="RELAY_3_REMAINING_CONTROL"/>
        <pointInfo name="Relay 3 Shed Time" init="true" attributes="RELAY_3_SHED_TIME_DATA_LOG"/>
        <pointInfo name="Relay 4 Remaining Control Time" init="true" attributes="RELAY_4_REMAINING_CONTROL"/>
        <pointInfo name="Relay 4 Shed Time" init="true" attributes="RELAY_4_SHED_TIME_DATA_LOG"/>
        <pointInfo name="Relay Circuit Fault" init="true"/>
        <pointInfo name="Runtime Load 1" init="true" attributes="RELAY_1_RUN_TIME_DATA_LOG"/>
        <pointInfo name="Runtime Load 2" init="true" attributes="RELAY_2_RUN_TIME_DATA_LOG"/>
        <pointInfo name="Runtime Load 3" init="true" attributes="RELAY_3_RUN_TIME_DATA_LOG"/>
        <pointInfo name="Runtime Load 4" init="true" attributes="RELAY_4_RUN_TIME_DATA_LOG"/>
        <pointInfo name="Runtime Tamper" init="true"/>
    </pointInfos>
    <commands>
        <command name="Last Interval kW Relay 1">
            <cmd text="getvalue interval last relay 1"/>
            <point name="Last Interval kW Relay 1"/>
        </command>
        <command name="Last Interval kW Relay 2">
            <cmd text="getvalue interval last relay 2"/>
            <point name="Last Interval kW Relay 2"/>
        </command>
        <command name="Last Interval kW Relay 3">
            <cmd text="getvalue interval last relay 3"/>
            <point name="Last Interval kW Relay 3"/>
        </command>
        <command name="Last Interval kW Relay 4">
            <cmd text="getvalue interval last relay 4"/>
            <point name="Last Interval kW Relay 4"/>
        </command>
        <command name="Prop Count">
            <cmd text="getvalue propcount"/>
            <point name="Prop Count"/>
        </command>
        <command name="Relay 1 Remaining Control Time">
            <cmd text="getvalue controltime remaining relay 1"/>
            <point name="Relay 1 Remaining Control Time"/>
        </command>
        <command name="Relay 1 Shed Time">
            <cmd text="getvalue shedtime relay 1 previous 12"/>
            <point name="Relay 1 Shed Time"/>
        </command>
        <command name="Relay 2 Remaining Control Time">
            <cmd text="getvalue controltime remaining relay 2"/>
            <point name="Relay 2 Remaining Control Time"/>
        </command>
        <command name="Relay 2 Shed Time">
            <cmd text="getvalue shedtime relay 2 previous 12"/>
            <point name="Relay 2 Shed Time"/>
        </command>
        <command name="Relay 3 Remaining Control Time">
            <cmd text="getvalue controltime remaining relay 3"/>
            <point name="Relay 3 Remaining Control Time"/>
        </command>
        <command name="Relay 3 Shed Time">
            <cmd text="getvalue shedtime relay 3 previous 12"/>
            <point name="Relay 3 Shed Time"/>
        </command>
        <command name="Relay 4 Remaining Control Time">
            <cmd text="getvalue controltime remaining relay 4"/>
            <point name="Relay 4 Remaining Control Time"/>
        </command>
        <command name="Relay 4 Shed Time">
            <cmd text="getvalue shedtime relay 4 previous 12"/>
            <point name="Relay 4 Shed Time"/>
        </command>
        <command name="Runtime Load 1">
            <cmd text="getvalue runtime load 1 previous 12"/>
            <point name="Runtime Load 1"/>
        </command>
        <command name="Runtime Load 2">
            <cmd text="getvalue runtime load 2 previous 12"/>
            <point name="Runtime Load 2"/>
        </command>
        <command name="Runtime Load 3">
            <cmd text="getvalue runtime load 3 previous 12"/>
            <point name="Runtime Load 3"/>
        </command>
        <command name="Runtime Load 4">
            <cmd text="getvalue runtime load 4 previous 12"/>
            <point name="Runtime Load 4"/>
        </command>
    </commands>
</pao>
