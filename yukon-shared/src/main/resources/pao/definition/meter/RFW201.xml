<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<pao xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" paoType="RFW201" changeGroup="rfw" displayGroup="RFMESH" displayName="RFW-201" xsi:noNamespaceSchemaLocation="../pao.xsd">
    <pointFiles>
        <pointFile>RFW.xml</pointFile>
    </pointFiles>
    <tags>
        <tag name="COMMANDER_REQUESTS"/>
        <tag name="DEVICE_CONFIGURATION"/>
        <tag name="DEVICE_ICON_TYPE" option="water_meter"/>
        <tag name="RFN_EVENTS"/>
        <tag name="RFN_POINT_CALCULATION"/>
        <tag name="WATER_METER_DETAIL_DISPLAYABLE"/>
    </tags>
    <configuration>
        <category type="rfnWaterChannelConfiguration"/>
    </configuration>
    <pointInfos>
        <pointInfo name="Battery End Of Life" attributes="BATTERY_END_OF_LIFE"/>
        <pointInfo name="Battery Voltage" init="true" attributes="BATTERY_VOLTAGE"/>
        <pointInfo name="Meter Read - Checksum error" attributes="METER_READ_CHECKSUM_ERROR"/>
        <pointInfo name="Meter Read - Error reading serial number" attributes="METER_READ_ERROR_READING_SERIAL_NUMBER"/>
        <pointInfo name="Meter Read - Field exceeded maximum digits" attributes="METER_READ_FIELD_EXCEEDED_MAXIMUM_DIGITS"/>
        <pointInfo name="Meter Read - No encoder found" attributes="METER_READ_NO_ENCODER_FOUND"/>
        <pointInfo name="Meter Read - No EOF detected" attributes="METER_READ_NO_EOF_DETECTED"/>
        <pointInfo name="Meter Read - Parity error" attributes="METER_READ_PARITY_ERROR"/>
        <pointInfo name="Meter Read - Protocol cannot be determined" attributes="METER_READ_PROTOCOL_CANNOT_BE_DETERMINED"/>
        <pointInfo name="Reverse Flow" attributes="REVERSE_FLOW"/>
        <pointInfo name="Suspected Leak" attributes="SUSPECTED_LEAK"/>
        <pointInfo name="Tamper - Cable Cut" attributes="TAMPER_CABLE_CUT"/>
        <pointInfo name="Temperature" init="true" attributes="TEMPERATURE_DEVICE"/>
        <pointInfo name="Temperature Out Of Range" attributes="TEMPERATURE_OUT_OF_RANGE"/>
        <pointInfo name="Thirty Days No Usage" attributes="THIRTY_DAYS_NO_USAGE"/>
        <pointInfo name="Water Usage" init="true" attributes="USAGE_WATER, USAGE"/>
        <pointInfo name="Water Usage per Interval" init="true" attributes="WATER_USAGE_PER_INTERVAL"/>
    </pointInfos>
</pao>
