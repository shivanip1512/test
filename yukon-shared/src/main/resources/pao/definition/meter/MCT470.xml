<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<pao xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" paoType="MCT470" changeGroup="meter" displayGroup="MCT" displayName="MCT-470" xsi:noNamespaceSchemaLocation="../pao.xsd">
    <pointFiles>
        <pointFile>MCT470.xml</pointFile>
    </pointFiles>
    <tags>
        <tag name="COMMANDER_REQUESTS"/>
        <tag name="DEVICE_CONFIGURATION"/>
        <tag name="DEVICE_ICON_TYPE" option="plc_electric_meter"/>
        <tag name="DLC_ADDRESS_RANGE" option="510001-560000"/>
        <tag name="DLC_ADDRESS_RANGE_ENFORCE" option="0-4194303"/>
        <tag name="LOAD_PROFILE"/>
        <tag name="LOCATE_ROUTE"/>
        <tag name="METER_DETAIL_DISPLAYABLE"/>
        <tag name="OUTAGE"/>
        <tag name="PLC_DEMAND_RESET"/>
        <tag name="PORTER_COMMAND_REQUESTS"/>
        <tag name="STARS_ACCOUNT_ATTACHABLE_METER"/>
        <tag name="THREE_PHASE_CURRENT"/>
        <tag name="THREE_PHASE_VOLTAGE"/>
        <tag name="TOU"/>
        <tag name="USES_METER_NUMBER_FOR_MSP"/>
    </tags>
    <configuration>
        <category type="addressing"/>
        <category type="demand"/>
        <category type="mct470ConfigurationByte"/>
        <category type="mct470PrecannedTable"/>
        <category type="mct470ProfileChannels"/>
        <category type="profile"/>
        <category type="relays"/>
        <category type="timeZone"/>
        <category type="tou"/>
    </configuration>
    <pointInfos>
        <pointInfo name="Average Power Factor" attributes="POWER_FACTOR"/>
        <pointInfo name="Blink Count" init="true"/>
        <pointInfo name="Comm Status" attributes="COMM_STATUS"/>
        <pointInfo name="IED Blink Count" init="true" attributes="BLINK_COUNT"/>
        <pointInfo name="IED Demand Reset Count" init="true" attributes="IED_DEMAND_RESET_COUNT"/>
        <pointInfo name="kVArh" init="true"/>
        <pointInfo name="kW" init="true"/>
        <pointInfo name="kW (Channel 2)"/>
        <pointInfo name="kW (Channel 3)"/>
        <pointInfo name="kW (Channel 4)"/>
        <pointInfo name="kW-LP" init="true" attributes="LOAD_PROFILE"/>
        <pointInfo name="kW-LP (Channel 2)" attributes="PROFILE_CHANNEL_2"/>
        <pointInfo name="kW-LP (Channel 3)" attributes="PROFILE_CHANNEL_3"/>
        <pointInfo name="kW-LP (Channel 4)" attributes="VOLTAGE_PROFILE"/>
        <pointInfo name="kWh" init="true"/>
        <pointInfo name="kWh (Channel 2)"/>
        <pointInfo name="kWh (Channel 3)"/>
        <pointInfo name="kWh (Channel 4)"/>
        <pointInfo name="Last Interval kVAr" attributes="KVAR"/>
        <pointInfo name="Last Interval kW" init="true" attributes="DEMAND"/>
        <pointInfo name="Minimum kW"/>
        <pointInfo name="Minimum kW (Channel 2)"/>
        <pointInfo name="Minimum kW (Channel 3)"/>
        <pointInfo name="Minimum kW (Channel 4)"/>
        <pointInfo name="Neutral Current" attributes="NEUTRAL_CURRENT"/>
        <pointInfo name="Outage Status" init="true" attributes="OUTAGE_STATUS"/>
        <pointInfo name="Peak kM (Coincidental)" attributes="PEAK_KVAR_COIN"/>
        <pointInfo name="Peak kVAr (Rate A kVAr)" init="true" attributes="PEAK_KVAR_RATE_A,PEAK_KVAR"/>
        <pointInfo name="Peak kVAr (Rate B kVAr)" attributes="PEAK_KVAR_RATE_B"/>
        <pointInfo name="Peak kVAr (Rate C kVAr)" attributes="PEAK_KVAR_RATE_C"/>
        <pointInfo name="Peak kVAr (Rate D kVAr)" attributes="PEAK_KVAR_RATE_D"/>
        <pointInfo name="Peak kW" init="true"/>
        <pointInfo name="Peak kW (Channel 2)"/>
        <pointInfo name="Peak kW (Channel 3)"/>
        <pointInfo name="Peak kW (Channel 4)"/>
        <pointInfo name="Peak kW (IED)"/>
        <pointInfo name="Peak kW (Rate A kW)" init="true" attributes="PEAK_DEMAND_RATE_A,PEAK_DEMAND"/>
        <pointInfo name="Peak kW (Rate B kW)" attributes="PEAK_DEMAND_RATE_B"/>
        <pointInfo name="Peak kW (Rate C kW)" attributes="PEAK_DEMAND_RATE_C"/>
        <pointInfo name="Peak kW (Rate D kW)" attributes="PEAK_DEMAND_RATE_D"/>
        <pointInfo name="Peak kW Frozen (Rate A kW)" attributes="PEAK_DEMAND_FROZEN_RATE_A,PEAK_DEMAND_FROZEN"/>
        <pointInfo name="Phase A Current" attributes="CURRENT_PHASE_A"/>
        <pointInfo name="Phase B Current" attributes="CURRENT_PHASE_B"/>
        <pointInfo name="Phase C Current" attributes="CURRENT_PHASE_C"/>
        <pointInfo name="Rate A kVArh" init="true" attributes="KVARH,KVARH_RATE_A"/>
        <pointInfo name="Rate A kWh" init="true" attributes="USAGE_RATE_A"/>
        <pointInfo name="Rate A kWh Frozen" attributes="USAGE_FROZEN"/>
        <pointInfo name="Rate B kVArh" attributes="KVARH_RATE_B"/>
        <pointInfo name="Rate B kWh" attributes="USAGE_RATE_B"/>
        <pointInfo name="Rate C kVArh" attributes="KVARH_RATE_C"/>
        <pointInfo name="Rate C kWh" attributes="USAGE_RATE_C"/>
        <pointInfo name="Rate D kVArh" attributes="KVARH_RATE_D"/>
        <pointInfo name="Rate D kWh" attributes="USAGE_RATE_D"/>
        <pointInfo name="Total kWh" init="true" attributes="USAGE"/>
        <pointInfo name="TOU Rate A (Channel 1)"/>
        <pointInfo name="TOU Rate A (Channel 2)"/>
        <pointInfo name="TOU Rate A - Peak kW (Channel 1)"/>
        <pointInfo name="TOU Rate A - Peak kW (Channel 2)"/>
        <pointInfo name="TOU Rate B (Channel 1)"/>
        <pointInfo name="TOU Rate B (Channel 2)"/>
        <pointInfo name="TOU Rate B - Peak kW (Channel 1)"/>
        <pointInfo name="TOU Rate B - Peak kW (Channel 2)"/>
        <pointInfo name="TOU Rate C (Channel 1)"/>
        <pointInfo name="TOU Rate C (Channel 2)"/>
        <pointInfo name="TOU Rate C - Peak kW (Channel 1)"/>
        <pointInfo name="TOU Rate C - Peak kW (Channel 2)"/>
        <pointInfo name="TOU Rate D (Channel 1)"/>
        <pointInfo name="TOU Rate D (Channel 2)"/>
        <pointInfo name="TOU Rate D - Peak kW (Channel 1)"/>
        <pointInfo name="TOU Rate D - Peak kW (Channel 2)"/>
        <pointInfo name="Voltage Phase A" attributes="VOLTAGE_PHASE_A"/> <!-- Not valid for Elster (Alpha) Power Plus -->
        <pointInfo name="Voltage Phase B" attributes="VOLTAGE_PHASE_B"/> <!-- Not valid for Elster (Alpha) Power Plus -->
        <pointInfo name="Voltage Phase C" attributes="VOLTAGE_PHASE_C"/> <!-- Not valid for Elster (Alpha) Power Plus -->
    </pointInfos>
    <commands>
        <command name="Cancel LP Request">
            <cmd text="getvalue lp cancel"/>
        </command>
        <command name="Read Blink Count">
            <cmd text="getvalue powerfail"/>
            <point name="Blink Count"/>
        </command>
        <command name="Read Current Demand">
            <cmd text="getvalue demand"/>
            <point name="kW"/>
            <point name="kW (Channel 2)"/>
            <point name="kW (Channel 3)"/>
            <point name="kW (Channel 4)"/>
        </command>
        <command name="Read Current Rate A kWh/Peak kW">
            <cmd text="getvalue ied current kwha"/>
            <point name="Peak kW (Rate A kW)"/>
            <point name="Rate A kWh"/>
        </command>
        <command name="Read Current Rate B kWh/Peak kW">
            <cmd text="getvalue ied current kwhb"/>
            <point name="Rate B kWh"/>
            <point name="Peak kW (Rate B kW)"/>
        </command>
        <command name="Read Current Rate C kWh/Peak kW">
            <cmd text="getvalue ied current kwhc"/>
            <point name="Peak kW (Rate C kW)"/>
            <point name="Rate C kWh"/>
        </command>
        <command name="Read Current Rate D kWh/Peak kW">
            <cmd text="getvalue ied current kwhd"/>
            <point name="Rate D kWh"/>
            <point name="Peak kW (Rate D kW)"/>
        </command>
        <command name="Read Frozen Peak" enabled="false">
            <cmd text="getvalue peak frozen"/>
            <point name="Peak kW"/>
            <point name="Minimum kW"/>
        </command>
        <command name="Read Frozen Peak (Channel 2)" enabled="false">
            <cmd text="getvalue peak offset 2 frozen"/>
            <point name="Peak kW (Channel 2)"/>
            <point name="Minimum kW (Channel 2)"/>
        </command>
        <command name="Read Frozen Peak (Channel 3)" enabled="false">
            <cmd text="getvalue peak offset 3 frozen"/>
            <point name="Peak kW (Channel 3)"/>
            <point name="Minimum kW (Channel 3)"/>
        </command>
        <command name="Read Frozen Peak (Channel 4)" enabled="false">
            <cmd text="getvalue peak offset 4 frozen"/>
            <point name="Peak kW (Channel 4)"/>
            <point name="Minimum kW (Channel 4)"/>
        </command>
        <command name="Read Frozen Rate A kWh/Peak kW" enabled="false">
            <cmd text="getvalue ied frozen kwha"/>
            <point name="Peak kW (Rate A kW)"/>
            <point name="Rate A kWh"/>
        </command>
        <command name="Read Frozen Rate B kWh/Peak kW" enabled="false">
            <cmd text="getvalue ied frozen kwhb"/>
            <point name="Rate B kWh"/>
            <point name="Peak kW (Rate B kW)"/>
        </command>
        <command name="Read Frozen Rate C kWh/Peak kW" enabled="false">
            <cmd text="getvalue ied frozen kwhc"/>
            <point name="Peak kW (Rate C kW)"/>
            <point name="Rate C kWh"/>
        </command>
        <command name="Read Frozen Rate D kWh/Peak kW" enabled="false">
            <cmd text="getvalue ied frozen kwhd"/>
            <point name="Rate D kWh"/>
            <point name="Peak kW (Rate D kW)"/>
        </command>
        <command name="Read IED Blink and Demand Reset Counts">
            <cmd text="getvalue ied outage"/>
            <point name="IED Demand Reset Count"/>
            <point name="IED Blink Count"/>
        </command>
        <command name="Read IED Last Interval Demands">
            <cmd text="getvalue ied demand"/>
            <point name="Last Interval kW"/>
        </command>
        <command name="Read IED Volts">
            <cmd text="getvalue ied demand"/>
            <point name="Voltage Phase A"/>
            <point name="Voltage Phase B"/>
            <point name="Voltage Phase C"/>
        </command>
        <command name="Read LP Request Status">
            <cmd text="getvalue lp status"/>
        </command>
        <command name="Read Peak">
            <cmd text="getvalue peak"/>
            <point name="Peak kW"/>
            <point name="Minimum kW"/>
        </command>
        <command name="Read Peak (Channel 2)">
            <cmd text="getvalue peak offset 2"/>
            <point name="Peak kW (Channel 2)"/>
            <point name="Minimum kW (Channel 2)"/>
        </command>
        <command name="Read Peak (Channel 3)">
            <cmd text="getvalue peak offset 3"/>
            <point name="Peak kW (Channel 3)"/>
            <point name="Minimum kW (Channel 3)"/>
        </command>
        <command name="Read Peak (Channel 4)">
            <cmd text="getvalue peak offset 4"/>
            <point name="Peak kW (Channel 4)"/>
            <point name="Minimum kW (Channel 4)"/>
        </command>
        <command name="Read Phase Current">
            <cmd text="getvalue phase current"/>
            <point name="Phase C Current"/>
            <point name="Phase A Current"/>
            <point name="Phase B Current"/>
            <point name="Neutral Current"/>
        </command>
        <command name="Read TOU Rate A Channel 1">
            <cmd text="getvalue kWh rate a"/>
            <point name="TOU Rate A - Peak kW (Channel 1)"/>
            <point name="TOU Rate A (Channel 1)"/>
        </command>
        <command name="Read TOU Rate A Channel 2">
            <cmd text="getvalue kWh rate a channel 2"/>
            <point name="TOU Rate A - Peak kW (Channel 2)"/>
            <point name="TOU Rate A (Channel 2)"/>
        </command>
        <command name="Read TOU Rate B Channel 1">
            <cmd text="getvalue kWh rate b"/>
            <point name="TOU Rate B (Channel 1)"/>
            <point name="TOU Rate B - Peak kW (Channel 1)"/>
        </command>
        <command name="Read TOU Rate B Channel 2">
            <cmd text="getvalue kWh rate b channel 2"/>
            <point name="TOU Rate B (Channel 2)"/>
            <point name="TOU Rate B - Peak kW (Channel 2)"/>
        </command>
        <command name="Read TOU Rate C Channel 1">
            <cmd text="getvalue kWh rate c"/>
            <point name="TOU Rate C (Channel 1)"/>
            <point name="TOU Rate C - Peak kW (Channel 1)"/>
        </command>
        <command name="Read TOU Rate C Channel 2">
            <cmd text="getvalue kWh rate c channel 2"/>
            <point name="TOU Rate C (Channel 2)"/>
            <point name="TOU Rate C - Peak kW (Channel 2)"/>
        </command>
        <command name="Read TOU Rate D Channel 1">
            <cmd text="getvalue kWh rate d"/>
            <point name="TOU Rate D - Peak kW (Channel 1)"/>
            <point name="TOU Rate D (Channel 1)"/>
        </command>
        <command name="Read TOU Rate D Channel 2">
            <cmd text="getvalue kWh rate d channel 2"/>
            <point name="TOU Rate D - Peak kW (Channel 2)"/>
            <point name="TOU Rate D (Channel 2)"/>
        </command>
        <command name="Read Total kWh">
            <cmd text="getvalue ied kwht"/>
            <point name="Total kWh"/>
        </command>
        <command name="Read Usage">
            <cmd text="getvalue kWh"/>
            <point name="kWh (Channel 2)"/>
            <point name="kWh"/>
            <point name="kWh (Channel 4)"/>
            <point name="kWh (Channel 3)"/>
        </command>
        <command name="Read Usage (Frozen)" enabled="false">
            <cmd text="getvalue kWh frozen"/>
            <point name="kWh (Channel 2)"/>
            <point name="kWh"/>
            <point name="kWh (Channel 3)"/>
        </command>
        <command name="Read rates for Demand, LP, Volt Demand, and Volt Profile">
            <cmd text="getconfig intervals"/>
        </command>
    </commands>
</pao>
