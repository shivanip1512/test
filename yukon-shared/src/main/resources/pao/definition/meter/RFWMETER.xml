        <pointFile>RFW.xml</pointFile>
    </pointFiles>
    <tags>
        <tag name="DEVICE_ICON_TYPE" option="water_meter"/>
        <tag name="RFN_POINT_CALCULATION"/>
        <tag name="SUPPORTS_ATTRIBUTE_ASSIGNMENT"/>
