    </point>
    <point offset="12" type="Analog">
        <name>Reset Alert Setpoint</name>
        <multiplier value="1"/>
        <unitofmeasure value="AMPS"/>
        <decimalplaces value="3"/>
    </point>
    <point offset="31" type="Analog">
        <name>Current Survey</name>
        <multiplier value="1"/>
        <unitofmeasure value="AMPS"/>
        <decimalplaces value="3"/>
    </point>
    <point offset="100" type="Analog">
        <name>RSSI</name>
        <multiplier value="1"/>
        <unitofmeasure value="COUNTS"/>
        <decimalplaces value="3"/>
    </point>
    <point offset="101" type="Analog">
        <name>BER</name>
        <multiplier value="1"/>
        <unitofmeasure value="UNDEF"/>
