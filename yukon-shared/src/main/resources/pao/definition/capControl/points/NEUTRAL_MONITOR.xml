        <unitofmeasure value="AMPS"/>
        <decimalplaces value="3"/>
    </point>
    <point offset="2" type="Analog">
        <name>Min Current - Report</name>
        <multiplier value="1"/>
        <unitofmeasure value="AMPS"/>
        <decimalplaces value="3"/>
