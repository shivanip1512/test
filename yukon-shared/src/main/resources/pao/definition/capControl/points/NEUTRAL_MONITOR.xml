    <point offset="1" type="Status">
        <name>Over Current</name>
        <stategroup value="TrueFalse"/>
    </point>
    <point offset="2" type="Status">
        <name>Reset Over Current</name>
        <stategroup value="TrueFalse"/>
    </point>
    <point offset="3" type="Status">
