<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<pao xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" paoType="RFN_RELAY" creatable="false" displayGroup="RFMESH" displayName="RFN Relay" xsi:noNamespaceSchemaLocation="../pao.xsd">
    <!-- Creatable=false means it cannot be created in DBEditor, nor listed as a type in device configurations...to-date  -->
    <pointFiles>
        <pointFile>RFN_RELAY.xml</pointFile>
    </pointFiles>
    <tags>
        <tag name="DB_EDITOR_INCOMPATIBLE"/>
        <tag name="DEVICE_ICON_TYPE" option="rfn_relay"/>
        <tag name="OUTAGE"/>
        <tag name="RELAY_DETAIL_DISPLAYABLE"/>
        <tag name="RFN_EVENTS"/>
    </tags>
    <pointInfos>
        <pointInfo name="Bad Upgrade Security Param" attributes="BAD_UPGRADE_SECURITY_PARAM"/>
        <pointInfo name="Blink Count" init="true" attributes="RFN_BLINK_COUNT"/>
        <pointInfo name="Blink Restore Count" init="true" attributes="RFN_BLINK_RESTORE_COUNT"/>
        <pointInfo name="Failed Upgrade Signature Verification" attributes="FAILED_UPGRADE_SIGNATURE_VERIF"/>
        <pointInfo name="Outage Count" init="true" attributes="RFN_OUTAGE_COUNT"/>
        <pointInfo name="Outages" init="true" attributes="OUTAGE_LOG"/>
        <pointInfo name="Outage Restore Count" init="true" attributes="RFN_OUTAGE_RESTORE_COUNT"/>
        <pointInfo name="Outage Status" init="true" attributes="OUTAGE_STATUS"/>
    </pointInfos>
</pao>
