project(loadmanagement) {
  exename=loadmanagement

  after += yukon-prebuild
  after += service
  after += ctimsg
  after += ctibase
  after += ctiholidaydb
  after += ctiseasondb
  after += ctidbsrc
  after += ctithriftmsg

