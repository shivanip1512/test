<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
            xmlns:yukon="http://yukon.cannontech.com/api" 
            targetNamespace="http://yukon.cannontech.com/api"
            elementFormDefault="qualified">
	
    <xsd:include schemaLocation="../common/Failure.xsd"/>
    <xsd:include schemaLocation="../common/Types.xsd"/>

    <xsd:annotation>
     	<xsd:documentation xml:lang="en">
      	STARS Customer Account modification result
     	</xsd:documentation>
    </xsd:annotation>
    
    <xsd:complexType name="CustomerAccountResultList">
        <xsd:sequence>
            <xsd:element name="customerAccount" maxOccurs="unbounded">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="accountNumber" type="yukon:AcountNumber"/>
                        <xsd:choice>
                            <xsd:element name="success" />
                            <xsd:element name="failure" type="yukon:Failure" />
                        </xsd:choice>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

</xsd:schema>