<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:yukon="http://yukon.cannontech.com/api" targetNamespace="http://yukon.cannontech.com/api">
	
<xsd:annotation>
 	<xsd:documentation xml:lang="en">
  	Integration II schema (integration with Retail Office) at BG&amp;E: Item #4, second INPUT option
 	*Yukon account number is Premise Number for BG&amp;E
 	</xsd:documentation>
</xsd:annotation>

<xsd:complexType name="TotalOverriddenDevicesByAccountNumberRequest">
  	<xsd:sequence maxOccurs="1" minOccurs="1">
	    <xsd:element name="accountNumber"   type="xsd:string"/>
	    <xsd:element name="startTime" type="xsd:dateTime"/>
	    <xsd:element name="stopTime" type="xsd:dateTime"/>
	</xsd:sequence>
</xsd:complexType>

<xsd:element name="totalOverriddenDevicesByAccountNumberRequest" type="yukon:TotalOverriddenDevicesByAccountNumberRequest"/>

</xsd:schema>
