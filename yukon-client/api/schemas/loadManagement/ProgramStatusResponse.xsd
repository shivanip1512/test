<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:yukon="http://yukon.cannontech.com/api" targetNamespace="http://yukon.cannontech.com/api">
	<xsd:include schemaLocation="ProgramStatus.xsd"/>
	<xsd:include schemaLocation="ErrorCode.xsd"/>
	<xsd:annotation>
		<xsd:documentation xml:lang="en">
  	ESO Single Screen schema for Integration I at BG&amp;E: Item #1 from Requirements Outline
 	</xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="ProgramStatusResponse">
		<xsd:sequence>
			<xsd:element name="programStatuses">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="programStatus" type="yukon:ProgramStatus" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="errorCode" type="yukon:ErrorCode"/>
			<xsd:element name="errorDescription" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="programStatusResponse" type="yukon:ProgramStatusResponse"/>
</xsd:schema>
