<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
            xmlns:yukon="http://yukon.cannontech.com/api" 
            targetNamespace="http://yukon.cannontech.com/api"
            elementFormDefault="qualified" version="1.0">

    <xsd:include schemaLocation="../../common/schemas/YukonMessage.xsd" />
    
    <xsd:element name="sendTextMessageResponse" type="yukon:SimpleSuccessOrFailure">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                This is the response returned for a sent text message.  The sending of a text message is done through the sendTextMessageRequest.
            </xsd:documentation>
        </xsd:annotation>
    </xsd:element>

</xsd:schema>