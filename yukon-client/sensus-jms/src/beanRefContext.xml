<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
 
 <beans>
   <bean id="com.cannontech.context.sensus-jms" 
        class="org.springframework.context.support.ClassPathXmlApplicationContext"
        lazy-init="true">
     <constructor-arg>
       <list>
         <value>com/cannontech/mainContext.xml</value>
         <value>com/cannontech/core/dao/applicationContext.xml</value>
         <value>com/cannontech/core/dynamic/applicationContext.xml</value>
         <value>com/cannontech/core/authentication/authContext.xml</value>
       </list>
     </constructor-arg>
   </bean>
   
 </beans>
