<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

	<bean id="csv2PointData" class="com.cannontech.datagenerator.CSV2PointData">
		<property name="dataSource"><ref bean="dynamicDataSource"/></property>
		<property name="timer"><ref bean="globalTimer"/></property>
		<property name="multiplier" value="1"/>
	</bean>
    
    <bean id="bulkMeterDeleter" class="com.cannontech.tools.bulk.BulkMeterDeleter">
		<property name="bMDS" ref="bulkMeterDeleterService"/>
	</bean>

</beans>