	<xs:import namespace="gml_V4.1_Release" schemaLocation="mspGeometry_V41_Release.xsd"/>
	<xs:element name="CPSM" type="cpsm:CPSM">
		<xs:annotation>
			<xs:documentation>Comment describing your root element</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="CPSM">
		<xs:sequence>
			<xs:element ref="cpsm:mspCIMObject" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="extensions" type="cpsm:extensions" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="documentType" type="cpsm:documentType" use="optional" default="incremental"/>
		<xs:attribute name="connectivityModel" type="cpsm:connectivityModel" use="optional"/>
	</xs:complexType>
	<!---->
	<!--The following objects are included to import functionality from MultiSpeak.-->
	<!---->
	<xs:simpleType name="connectivityModel">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Nodal"/>
			<xs:enumeration value="Sectional"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="documentType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="incremental"/>
			<xs:enumeration value="dump"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="extensions" type="cpsm:extensions"/>
	<xs:complexType name="extensions">
		<xs:annotation>
			<xs:documentation>This container should be used to add any extensions to the attached object when the object is sent in a batch transfer.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:any namespace="##any" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="extensionsItem">
		<xs:sequence>
			<xs:element name="extName" type="xs:string"/>
			<xs:element name="extValue" type="xs:string"/>
			<xs:element name="extType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>It is suggested that the type be one of the xs:data types.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="anySimpleType"/>
						<xs:enumeration value="anyType"/>
						<xs:enumeration value="anyURI"/>
						<xs:enumeration value="base64Binary"/>
						<xs:enumeration value="boolean"/>
						<xs:enumeration value="byte"/>
						<xs:enumeration value="date"/>
						<xs:enumeration value="dateTime"/>
						<xs:enumeration value="decimal"/>
						<xs:enumeration value="double"/>
						<xs:enumeration value="duration"/>
						<xs:enumeration value="ENTITIES"/>
						<xs:enumeration value="ENTITY"/>
						<xs:enumeration value="float"/>
						<xs:enumeration value="gDay"/>
						<xs:enumeration value="gMonth"/>
						<xs:enumeration value="gMonthDay"/>
						<xs:enumeration value="gYear"/>
						<xs:enumeration value="gYearMonth"/>
						<xs:enumeration value="hexBinary"/>
						<xs:enumeration value="ID"/>
						<xs:enumeration value="IDREF"/>
						<xs:enumeration value="IDREFS"/>
						<xs:enumeration value="int"/>
						<xs:enumeration value="integer"/>
						<xs:enumeration value="language"/>
						<xs:enumeration value="long"/>
						<xs:enumeration value="Name"/>
						<xs:enumeration value="NCName"/>
						<xs:enumeration value="negativeInteger"/>
						<xs:enumeration value="NMTOKEN"/>
						<xs:enumeration value="NMTOKENS"/>
						<xs:enumeration value="nonNegativeInteger"/>
						<xs:enumeration value="nonPositiveInteger"/>
						<xs:enumeration value="normalizedString"/>
						<xs:enumeration value="NOTATION"/>
						<xs:enumeration value="positiveInteger"/>
						<xs:enumeration value="QName"/>
						<xs:enumeration value="short"/>
						<xs:enumeration value="string"/>
						<xs:enumeration value="time"/>
						<xs:enumeration value="token"/>
						<xs:enumeration value="unsignedByte"/>
						<xs:enumeration value="unsignedInt"/>
						<xs:enumeration value="unsignedLong"/>
						<xs:enumeration value="unsignedShort"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="extensionsList">
		<xs:annotation>
			<xs:documentation>This container should be used to add any extensions to the attached object when the object is sent by a web services method.</xs:documentation>
		</xs:annotation>
		<xs:sequence minOccurs="0" maxOccurs="unbounded">
			<xs:element name="extensionsItem" type="cpsm:extensionsItem"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="objectID">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="action">
		<xs:annotation>
			<xs:documentation>MultiSpeak verbs</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="New"/>
			<xs:enumeration value="Change"/>
			<xs:enumeration value="Delete"/>
			<xs:enumeration value="Replace"/>
			<xs:enumeration value="Link"/>
			<xs:enumeration value="Unlink"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="nodeIdentifier">
		<xs:annotation>
			<xs:documentation>Identifies node in an enginnering model.  Name of the node is required, objectID is optional and may be used to persist this information.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="name" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Name for engineering node.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="objectID" type="cpsm:objectID" use="optional">
			<xs:annotation>
				<xs:documentation>This is a unique identifier for thehis node.  If a table of connectivityNode objects is included, then this objectID is the objectID of the entries in that table.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="objectRef">
		<xs:annotation>
			<xs:documentation>Reference to an instance of an object.  When used to describe an engineering line section, this object must contain a section name and may also contain a noun/objectID pair that will uniquely identify the MultiSpeak noun (element) and instance of that noun that is contained in the named section.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="name" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Name of instance of the object.  When used to describe an engineering line section, the name is the name of the section and is required.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="noun" type="xs:QName" use="optional">
			<xs:annotation>
				<xs:documentation>The noun is the name of the object type that includes this instance.  For example, if instance is described by the MultiSpeak "substation" object then this field will contain "substation".</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="objectID" type="cpsm:objectID" use="optional">
			<xs:annotation>
				<xs:documentation>This is the identifier for the instance of the object.  objectID must be unique over the domain of the specific type of object.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="utility" type="xs:string">
			<xs:annotation>
				<xs:documentation>This is a tring identifier for the the utility that owns ththe identified object.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="voltage">
		<xs:simpleContent>
			<xs:extension base="xs:float">
				<xs:attribute name="units" type="cpsm:voltageUnits">
					<xs:annotation>
						<xs:documentation>The units for this voltage.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="current">
		<xs:simpleContent>
			<xs:extension base="xs:float">
				<xs:attribute name="units" type="cpsm:currentUnits">
					<xs:annotation>
						<xs:documentation>Units in which this current is measured.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="realPower">
		<xs:simpleContent>
			<xs:extension base="xs:float">
				<xs:attribute name="units" type="cpsm:realPowerUnits">
					<xs:annotation>
						<xs:documentation>Units for real power.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="reactivePower">
		<xs:simpleContent>
			<xs:extension base="xs:float">
				<xs:attribute name="units" type="cpsm:reactivePowerUnits">
					<xs:annotation>
						<xs:documentation>Units for reactive power.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="apparentPower">
		<xs:simpleContent>
			<xs:extension base="xs:float">
				<xs:attribute name="units" type="cpsm:apparentPowerUnits">
					<xs:annotation>
						<xs:documentation>Units of apparent power.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="activePower">
		<xs:annotation>
			<xs:documentation>Product of RMS value of the voltage and the RMS value of the in-phase component of the current (Megawatt)</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="xs:float">
				<xs:attribute name="units" type="cpsm:realPowerUnits">
					<xs:annotation>
						<xs:documentation>Units of active power</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="resistance">
		<xs:simpleContent>
			<xs:extension base="xs:float">
				<xs:attribute name="units" type="cpsm:zUnit">
					<xs:annotation>
						<xs:documentation>Units for resistance.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="resistancePerUnitLength">
		<xs:simpleContent>
			<xs:extension base="xs:float">
				<xs:attribute name="resistanceUnits" type="cpsm:zUnit">
					<xs:annotation>
						<xs:documentation>Unit for resistance.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="lengthUnits" type="cpsm:lengthUnit">
					<xs:annotation>
						<xs:documentation>Units for the reference length.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="reactance">
		<xs:simpleContent>
			<xs:extension base="xs:float">
				<xs:attribute name="units" type="cpsm:zUnit">
					<xs:annotation>
						<xs:documentation>Units of reactance.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="impedance">
		<xs:simpleContent>
			<xs:extension base="xs:float">
				<xs:attribute name="units" type="cpsm:zUnit">
					<xs:annotation>
						<xs:documentation>Units of impedance.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="complexImpedance">
		<xs:sequence>
			<xs:element name="r" type="xs:float" minOccurs="0"/>
			<xs:element name="x" type="xs:float" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="units" type="cpsm:zUnit">
			<xs:annotation>
				<xs:documentation>Units for this impedance.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="admittance">
		<xs:simpleContent>
			<xs:extension base="xs:float">
				<xs:attribute name="units" type="cpsm:sUnits">
					<xs:annotation>
						<xs:documentation>Units of impedance.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="susceptance">
		<xs:simpleContent>
			<xs:extension base="xs:float">
				<xs:attribute name="units" type="cpsm:sUnits">
					<xs:annotation>
						<xs:documentation>Units of susceptance.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="conductance">
		<xs:simpleContent>
			<xs:extension base="xs:float">
				<xs:attribute name="units" type="cpsm:sUnits">
					<xs:annotation>
						<xs:documentation>Units of conductance.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="capacitance">
		<xs:simpleContent>
			<xs:extension base="xs:float">
				<xs:attribute name="units" type="cpsm:capacitanceUnits">
					<xs:annotation>
						<xs:documentation>Units of capacitance.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="capacitancePerUnitLength">
		<xs:simpleContent>
			<xs:extension base="xs:float">
				<xs:attribute name="capacitanceUnits" type="cpsm:capacitanceUnits"/>
				<xs:attribute name="lengthUnits" type="cpsm:lengthUnit"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="resistivity">
		<xs:simpleContent>
			<xs:extension base="xs:float">
				<xs:attribute name="units" type="cpsm:resistivityUnits">
					<xs:annotation>
						<xs:documentation>Units of resistivity</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="temperature">
		<xs:simpleContent>
			<xs:extension base="xs:float">
				<xs:attribute name="units" type="cpsm:temperatureUnits">
					<xs:annotation>
						<xs:documentation>Units of temperature</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="thermalCoefficientOfResistance">
		<xs:simpleContent>
			<xs:extension base="xs:float">
				<xs:attribute name="units" type="cpsm:thermalCoefficientOfResistanceUnits">
					<xs:annotation>
						<xs:documentation>Units of temperature coefficient.  It is suggested that temperatures are expressed in degrees C and that the coefficient is expressed in reciprocal degrees C.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="pressure">
		<xs:simpleContent>
			<xs:extension base="xs:float">
				<xs:attribute name="units" type="cpsm:pressureUnits">
