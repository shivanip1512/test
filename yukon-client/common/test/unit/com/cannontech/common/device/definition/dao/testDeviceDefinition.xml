            <point type="DemandAccumulator">
                <attribute name="demand" />
                <name>Total kW</name>
                <offset value="11" />
                <multiplier value="1.0" />
                <unitofmeasure value="0" />
            </point>
            <point type="DemandAccumulator">
                <attribute name="rateADemand" />
                <name>Rate A kW</name>
                <offset value="12" />
                <multiplier value="1.0" />
                <unitofmeasure value="0" />
            </point>
            <point type="status">
            	<attribute name="status" />
                <name>Status A</name>
                <offset value="123" />
                <stategroup value="-6" />
            </point>
        </points>
    </device>
</deviceDefinition>
