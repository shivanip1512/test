<?xml version="1.0"?>
<deviceDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="deviceAttributes.xsd">
    <family id="MCT4xx">
        <points>
            <point type="Analog">
                <attribute name="analog1" />
                <name>Analog 1</name>
                <offset value="1" />
                <multiplier value="1.0" />
                <unitofmeasure value="1" />
            </point>
            <point type="Analog">
                <name>Analog 2</name>
                <offset value="2" />
                <multiplier value="1.0" />
                <unitofmeasure value="0" />
            </point>
        </points>
    </family>
    <device id="MCT410CL">
        <family-ref ref="MCT4xx" />
        <type value="MCT410CL" />
        <points>
            <point type="PulseAccumulator" init="true">
                <attribute name="usage" />
                <name>Total kWh</name>
                <offset value="1" />
                <multiplier value="1.0" />
