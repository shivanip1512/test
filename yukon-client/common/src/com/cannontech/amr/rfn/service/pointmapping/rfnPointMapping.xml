<?xml version="1.0" encoding="UTF-8"?>
<pointMappings>
    <pointGroup>
        <paoType value="RFN410FL"/>
        <paoType value="RFN410FX"/>
        <paoType value="RFN410FD"/>
        <point name="Delivered kWh"> 
            <uom>Wh</uom>
            <multiplier value=".001"/>
            <parseSiPrefix/>
            <modifiers match="all">
                <modifier>Quadrant 1</modifier>
                <modifier>Quadrant 4</modifier>
            </modifiers>
            <modifiers match="none">
                <modifier>Quadrant 2</modifier>
                <modifier>Quadrant 3</modifier>
            </modifiers>
        </point>
        <point name="Received kWh"> 
            <uom>Wh</uom>
            <multiplier value=".001"/>
            <parseSiPrefix/>
            <modifiers match="all">
                <modifier>Quadrant 2</modifier>
                <modifier>Quadrant 3</modifier>
            </modifiers>
            <modifiers match="none">
                <modifier>Quadrant 1</modifier>
                <modifier>Quadrant 4</modifier>
            </modifiers>
        </point>
