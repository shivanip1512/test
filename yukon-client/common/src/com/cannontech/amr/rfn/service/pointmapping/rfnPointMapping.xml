            <modifiers match="all">
                <modifier>Quadrant 1</modifier>
                <modifier>Quadrant 4</modifier>
            </modifiers>
            <modifiers match="none">
                <modifier>Quadrant 2</modifier>
                <modifier>Quadrant 3</modifier>
            </modifiers>
