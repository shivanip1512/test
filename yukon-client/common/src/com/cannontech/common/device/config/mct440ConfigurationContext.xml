    <bean id="mct440Configuration"
        class="com.cannontech.common.device.config.model.CategoryTemplate">
        <property name="name" value="MCT Configuration" />
        <property name="inputList">
            <list>
                <bean class="com.cannontech.web.input.InputBase">
                    <property name="displayName" value="Enable DST" />
                    <property name="field" value="enableDst" />
                    <property name="description">
                        <value>
                            If this is set, the device will action any DST changes. Clearing
                            this will force the DST Active flag to zero.
                        </value>
                    </property>
                    <property name="type" ref="inputBooleanType" />
                    <property name="security" ref="configEditSecurity" />
                </bean>

                <bean class="com.cannontech.web.input.InputBase">
                    <property name="displayName" value="Time Adjust Tolerance" />
                    <property name="field" value="timeAdjustTolerance" />
                    <property name="description">
                        <value>
                            Update the RTC if it is this far away from the incoming time sync
                        </value>
                    </property>
                    <property name="type" ref="inputIntegerType" />
                    <property name="validatorList">
                        <list>
                            <bean class="com.cannontech.web.input.validate.IntegerRangeValidator">
                                <property name="minValue" value="15" />
                                <property name="maxValue" value="255" />
                            </bean>
                        </list>
                    </property>
                    <property name="security" ref="configEditSecurity" />
                </bean>
            </list>
        </property>
    </bean>

