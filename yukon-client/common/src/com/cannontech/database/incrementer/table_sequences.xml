<sequencetables>
  <!--  set initsequence="true" on the table element to initialize the sequence
        with the max value from the identity column
    -->
  <sequencetable name="SequenceNumber" valuecolumn="lastValue" keycolumn="sequenceName">
      <sequence name="general" >
        <table name="CCurtProgram" identitycolumn="CCurtProgramId"/>
        <table name="CCurtProgramType" identitycolumn="CCurtProgramTypeId"/>
        <table name="CCurtGroup" identitycolumn="CCurtGroupId"/>
        <table name="CCurtProgramGroup" identitycolumn="CCurtProgramGroupId"/>
        <table name="CCurtGroupCustomerNotif" identitycolumn="CCurtGroupCustomerNotifId"/>
        <table name="CICustomerBase" identitycolumn="CustomerID"/>
        <table name="CCurtCurtailmentEventNotif" identitycolumn="CCurtCurtailmentEventId"/>
        <table name="CCurtCurtailmentEventParticipant" identitycolumn="CCurtCurtailmentEventParticipantId"/>
        <table name="CCurtCurtailmentEvent" identitycolumn="CCurtCurtailmentEventId"/>
        <table name="CCurtProgramParameter" identitycolumn="CCurtProgramParameterId"/>
        <table name="CCurtEconomicEvent" identitycolumn="CCurtEconomicEventId"/>
        <table name="CCurtEconomicEventNotif" identitycolumn="CCurtEconomicEventNotifId"/>
        <table name="CCurtEconomicEventParticipant" identitycolumn="CCurtEconomicEventParticipantId"/>
        <table name="CCurtEEParticipantSelection" identitycolumn="CCurtEEParticipantSelectionId"/>
        <table name="CCurtEEParticipantWindow" identitycolumn="CCurtEEParticipantWindowId"/>
        <table name="CCurtEEPricing" identitycolumn="CCurtEEPricingId"/>
        <table name="CCurtEEPricingWindow" identitycolumn="CCurtEEPricingWindowId"/>
      </sequence>
      <sequence name="pointid">
        <table name="Point" identitycolumn="POINTID" initsequence="true"/>
      </sequence>
  </sequencetable>
</sequencetables>