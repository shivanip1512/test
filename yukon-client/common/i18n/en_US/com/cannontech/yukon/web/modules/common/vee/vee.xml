    <entry key="yukon.web.modules.common.vee.monitor.editor.popupInfo.validationMonitoringEnable"><![CDATA[
         Enable validation monitoring:
         <br><br>
         Incoming data will be validated using the rules of this monitor.
         <br><br>
    ]]></entry>
    <entry key="yukon.web.modules.common.vee.monitor.editor.label.validationMonitoringDisable">Disable</entry>
