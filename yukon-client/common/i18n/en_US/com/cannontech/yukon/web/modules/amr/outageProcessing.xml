    <entry key="yukon.web.modules.amr.outageMonitorConfig.label.outageMonitoringEnable">Enable</entry>
    <entry key="yukon.web.modules.amr.outageMonitorConfig.popupInfo.outageMonitoringEnable"><![CDATA[
    	 <br>
	     Enable outage monitoring.
	     <br><br>
	     Devices found to have excessive outages will be added to the Outages Group.
	     <br><br>
    ]]></entry>
    <entry key="yukon.web.modules.amr.outageMonitorConfig.label.outageMonitoringDisable">Disable</entry>
    <entry key="yukon.web.modules.amr.outageMonitorConfig.popupInfo.outageMonitoringDisable"><![CDATA[
    	 <br>
	     Disable outage monitoring.
	     <br><br>
	     Devices will not be monitored for outage count violations and will no longer be added to the Outages Device Group for this monitor.
	     <br><br>
    ]]></entry>
    <entry key="yukon.web.modules.amr.outageMonitorConfig.label.update">Update</entry>
    <entry key="yukon.web.modules.amr.outageMonitorConfig.label.update.busy">Updating</entry>
    <entry key="yukon.web.modules.amr.outageMonitorConfig.label.create">Save</entry>
    <entry key="yukon.web.modules.amr.outageMonitorConfig.label.create.busy">Saving</entry>
    <entry key="yukon.web.modules.amr.outageMonitorConfig.label.delete">Delete</entry>
    <entry key="yukon.web.modules.amr.outageMonitorConfig.label.delete.busy">Deleting</entry>
    <entry key="yukon.web.modules.amr.outageMonitorConfig.label.cancel">Cancel</entry>
    <entry key="yukon.web.modules.amr.outageMonitorConfig.deleteConfirm">Are you sure you want to delete this Outage Monitor?</entry>
    <entry key="yukon.web.modules.amr.outageMonitorConfig.popupInfo.deviceGroup"><![CDATA[
    	 <br>
	     The group of devices that will be monitored for outage count violations.
	     <br><br>
	     Devices that meet the outage count requirements will be added to the Outages Device Group for this monitor.
	     <br><br>
    ]]></entry>
    <entry key="yukon.web.modules.amr.outageMonitorConfig.popupInfo.numberOfOutages"><![CDATA[
    	 <br>
	     Minimum number of outages required to add a device to the Outages Device Group for this monitor.
	     <br><br>
	     Must be greater than or equal to 1. Number is inclusive.
	     <br><br>
    ]]></entry>
    <entry key="yukon.web.modules.amr.outageMonitorConfig.popupInfo.timePeriod"><![CDATA[
    	 <br>
	     Minimum number of days between outage count readings.
	     <br><br>
	     Must be greater than 0.
	     <br><br>
    ]]></entry>
    <entry key="yukon.web.modules.amr.outageMonitorConfig.popupInfo.scheduleRead"><![CDATA[
    	 <br>
	     Outage count read commands will be sent to each of the devices in the selected device group periodically based on the Read Time/Frequency.
	     <br><br>
    ]]></entry>
    <entry key="yukon.web.modules.amr.outageMonitorConfig.popupInfo.scheduleReadNoteLabelText">Note:</entry>
    <entry key="yukon.web.modules.amr.outageMonitorConfig.popupInfo.scheduleReadNoteBodyText"><![CDATA[
	     Creating a scheduled read at this time does not link the schedule to this Outage Monitor in any way, but instead is simply a shortcut to add a scheduled blink count read at this time.
    ]]></entry>
    <entry key="yukon.web.modules.amr.outageMonitorConfig.saveOk">Outage Monitor Has Been Saved</entry>
    
