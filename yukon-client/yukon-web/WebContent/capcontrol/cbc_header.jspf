				            <option>Load Response</option>
				            <option>Metering</option>
				            <option>Work Orders</option>
                          </select>
                          <a href="x" class="menuLink">Help</a> 
                          <a href="/servlet/LoginController?ACTION=LOGOUT" class="menuLink">Log Out</a>
						</div>
