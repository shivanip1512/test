              </tr>
              <tr> 
                <td height="1" class="trimBGColor"></td>
              </tr>
              <tr>
			
			<form id="subMenuFrm" action="results.jsp" method="post">
			<input type="hidden" name="type">
			<input type="hidden" name="value">
                <td id="subMenu" class="secondBGColor" height="20">
				  <div class="subMenuPopup">
					<a href="#" class="submenuLink" onclick="wizardPost('<%=DBEditorForm.getWizardURL(PAOGroups.CAP_CONTROL_SUBBUS)%>');">New Substation</a>
					<a href="#" class="submenuLink" onclick="wizardPost('<%=DBEditorForm.getWizardURL(PAOGroups.CAP_CONTROL_FEEDER)%>');">New Feeder</a> 
					<a href="#" class="submenuLink" onclick="wizardPost('<%=DBEditorForm.getWizardURL(PAOGroups.CAPBANK)%>');">New CapBank</a> 
