        <tr> 
          <td> 
            <table width="100%" border="0" cellspacing="0" cellpadding="0" align="center" height="53">
              <tr> 
                <td width="63%"><img src="images/YourLogo.gif" /></td>
                <td width="37%"><div class="rAlign">
                  <img src="images/HeaderGraphic.gif" /></div>
                </td>
              </tr>
            </table>
            <table width="100%" border="0" cellspacing="0" cellpadding="0" height="43">
              <tr> 
                <td height="1" class="trimBGColor"></td>
              </tr>
              <tr> 
                <td class="primeBGColor"> 
                  <table width="100%" border="0" cellspacing="0" cellpadding="0">
                    <tr> 
                      <td width="51%" height="23">
	                      <div id="sMenu">
							<cti:isPropertyTrue propertyid="<%= CBCSettingsRole.CBC_DATABASE_EDIT %>">
	                      		<a href="#" class="menuLink" onclick="menuShow('subMenu', 0); return false;">Create</a>
							</cti:isPropertyTrue>	                        
							<cti:isPropertyTrue propertyid="<%= CBCSettingsRole.CBC_DATABASE_EDIT %>">
		                        <a href="#" class="menuLink" onclick="menuShow('subMenu', 1); return false;">Modify</a>
							</cti:isPropertyTrue>	                        
	                        <a href="#" class="menuLink" onclick="menuShow('subMenu', 2); return false;">Reporting</a>
	                        <a href="#" class="menuLink" onclick="menuShow('subMenu', 3); return false;">View</a>
