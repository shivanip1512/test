<?xml version="1.0"?>

<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config>

    
    <!-- Database specific (DBPersistent) managed beans -->
    <managed-bean>
        <managed-bean-name>capControlForm</managed-bean-name>
        <managed-bean-class>com.cannontech.web.editor.CapControlForm</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
        
        <managed-property>
            <property-name>seasonScheduleDao</property-name>
            <value>#{seasonScheduleDao}</value>
        </managed-property>
        
        <managed-property>
            <property-name>holidayScheduleDao</property-name>
            <value>#{holidayScheduleDao}</value>
        </managed-property>
        
        <managed-property>
            <property-name>strategyDao</property-name>
            <value>#{strategyDao}</value>
        </managed-property>
        
        <managed-property>
        	<property-name>scheduleDao</property-name>
        	<value>#{scheduleDao}</value>
        </managed-property>
        
        <managed-property>
            <property-name>capbankControllerDao</property-name>
            <value>#{capbankControllerDao}</value>
        </managed-property>
        
        <managed-property>
            <property-name>capControlCreationService</property-name>
            <value>#{capControlCreationService}</value>
        </managed-property>
        
        <managed-property>
            <property-name>selectionLists</property-name>
            <value>#{selLists}</value>
        </managed-property>
        
        <managed-property>
            <property-name>capControlCache</property-name>
            <value>#{capControlCache}</value>
        </managed-property>
        
        <managed-property>
            <property-name>capbankDao</property-name>
            <value>#{capbankDao}</value>
        </managed-property>

        <managed-property>
            <property-name>feederDao</property-name>
            <value>#{feederDao}</value>
        </managed-property>

        <managed-property>
            <property-name>substationBusDao</property-name>
            <value>#{substationBusDao}</value>
        </managed-property>

        <managed-property>
            <property-name>paoDao</property-name>
            <value>#{paoDao}</value>
        </managed-property>

        <managed-property>
            <property-name>pointDao</property-name>
            <value>#{pointDao}</value>
        </managed-property>

        <managed-property>
            <property-name>rolePropertyDao</property-name>
            <value>#{rolePropertyDao}</value>
        </managed-property>

        <managed-property>
            <property-name>nextValueHelper</property-name>
            <value>#{nextValueHelper}</value>
        </managed-property>

        <managed-property>
            <property-name>transactionTemplate</property-name>
            <value>#{transactionTemplate}</value>
        </managed-property>
    </managed-bean>
    
     <managed-bean>
        <managed-bean-name>ptEditorForm</managed-bean-name>
        <managed-bean-class>com.cannontech.web.editor.point.PointForm</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>capBankEditor</managed-bean-name>
        <managed-bean-class>com.cannontech.web.editor.CapBankEditorForm</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>paoDeleteForm</managed-bean-name>
        <managed-bean-class>com.cannontech.web.delete.DeleteFormPAO</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>pointDeleteForm</managed-bean-name>
        <managed-bean-class>com.cannontech.web.delete.DeleteFormPoint</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>paoScheduleForm</managed-bean-name>
        <managed-bean-class>com.cannontech.web.editor.PAOScheduleForm</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>cbcCopyForm</managed-bean-name>
        <managed-bean-class>com.cannontech.web.copy.DBCopyForm</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <!-- page navigation rules -->
    <navigation-rule>
        <navigation-case>
            <from-outcome>paoSchedList</from-outcome>
            <to-view-id>/editor/paoSchedListBase.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <navigation-case>
            <from-outcome>pointEditor</from-outcome>
            <to-view-id>/editor/pointBase.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <converter>
        <converter-id>controlAlgorithmConverter</converter-id>
        <converter-class>com.cannontech.web.capcontrol.converter.ControlAlgorithmConverter</converter-class>
    </converter>
    
    <converter>
        <converter-id>controlMethodConverter</converter-id>
        <converter-class>com.cannontech.web.capcontrol.converter.ControlMethodConverter</converter-class>
    </converter>
    
</faces-config>