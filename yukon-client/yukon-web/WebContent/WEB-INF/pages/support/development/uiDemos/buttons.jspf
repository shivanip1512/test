    &lt;li&gt;
        &lt;a href=&quot;javascript:void(0);&quot; class=&quot;clearfix&quot;&gt;&lt;cti:icon icon=&quot;icon-pencil&quot;/&gt;&lt;span class=&quot;fl dib&quot;&gt;Edit&lt;/span&gt;&lt;/a&gt;
