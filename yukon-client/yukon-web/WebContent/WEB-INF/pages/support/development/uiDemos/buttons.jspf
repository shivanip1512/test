    &lt;li&gt;
        &lt;a href=&quot;javascript:void(0);&quot; class=&quot;clearfix&quot;&gt;&lt;cti:icon icon=&quot;icon-page-copy&quot;/&gt;&lt;span class=&quot;fl dib&quot;&gt;Copy&lt;/span&gt;&lt;/a&gt;
    &lt;/li&gt;
    &lt;li&gt;
        &lt;a href=&quot;javascript:void(0);&quot; class=&quot;clearfix&quot;&gt;&lt;cti:icon icon=&quot;icon-cross&quot;/&gt;&lt;span class=&quot;fl dib&quot;&gt;Delete&lt;/span&gt;&lt;/a&gt;
