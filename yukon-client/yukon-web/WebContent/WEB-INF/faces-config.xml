<?xml version="1.0"?>

<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config>

	<!-- Our DBCache object -->
    <managed-bean>
        <managed-bean-name>dbCache</managed-bean-name>
        <managed-bean-class>com.cannontech.database.cache.DefaultDatabaseCache</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>selLists</managed-bean-name>
        <managed-bean-class>com.cannontech.web.util.CBCSelectionLists</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>



	
	<!-- Database specific (DBPersistent) managed beans -->
    <managed-bean>
        <managed-bean-name>ptEditorForm</managed-bean-name>
        <managed-bean-class>com.cannontech.web.editor.point.PointForm</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>capControlForm</managed-bean-name>
        <managed-bean-class>com.cannontech.web.editor.CapControlForm</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>paoDeleteForm</managed-bean-name>
        <managed-bean-class>com.cannontech.web.delete.DeleteFormPAO</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>pointDeleteForm</managed-bean-name>
        <managed-bean-class>com.cannontech.web.delete.DeleteFormPoint</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>paoScheduleForm</managed-bean-name>
        <managed-bean-class>com.cannontech.web.editor.PAOScheduleForm</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    



	<!-- Navigation bean -->
    <managed-bean>
        <managed-bean-name>CtiNavObject</managed-bean-name>
        <managed-bean-class>com.cannontech.web.navigation.CtiNavObject</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>


	<!-- Converts HH:mm strings into total seconds -->
	<converter>
		<converter-id>cti_TimeConverter</converter-id> 
		<converter-class>com.cannontech.web.util.TimeConverter</converter-class> 
	</converter> 
    
    
    

	<!-- page navigation rules -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>cbcEditor</from-outcome>
			<to-view-id>/editor/cbcBase.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<navigation-case>
			<from-outcome>paoSchedList</from-outcome>
			<to-view-id>/editor/paoSchedListBase.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>


</faces-config>
        