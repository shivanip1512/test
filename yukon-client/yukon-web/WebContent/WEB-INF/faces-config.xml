<?xml version="1.0"?>

<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config>

	<!-- Our DBCache object -->
    <managed-bean>
        <managed-bean-name>dbCache</managed-bean-name>
        <managed-bean-class>com.cannontech.database.cache.DefaultDatabaseCache</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
	
	
	<!-- Database specific (DBPersistent) managed beans -->
    <managed-bean>
        <managed-bean-name>ptEditorForm</managed-bean-name>
        <managed-bean-class>com.cannontech.web.editor.PointForm</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>



	<!-- navigation rules for helloWorld.jsp -->
	<navigation-rule>
		<from-view-id>/helloWorld.jsp</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/page2.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- navigation rules for page2.jsp -->
	<navigation-rule>
		<from-view-id>/page2.jsp</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/helloWorld.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
