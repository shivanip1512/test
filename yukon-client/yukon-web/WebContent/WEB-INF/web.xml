	<context-param>
		<param-name>locatorFactorySelector</param-name>
		<param-value>classpath*:beanRefContext.xml</param-value>
	</context-param>

	<context-param>
        <!-- In addition to its normal Spring meaning, this is used to set the default context on 
             the YukonSpringHook. -->
		<param-name>parentContextKey</param-name>
		<param-value>com.cannontech.context.web</param-value>
	</context-param>

	<context-param>
		<param-name>contextClass</param-name>
        <!-- A special class is used here to override the default location of the .xml files. -->
		<param-value>com.cannontech.spring.CannonXmlWebApplicationContext</param-value>
	</context-param>

	<context-param>
        <!-- This is read by the CannonContextLoaderListener and then used to set a system property
             with the same name and value. -->
		<param-name>cti.app.name</param-name>
		<param-value>Webserver</param-value>
	</context-param>

	<context-param>
		<param-name>javax.faces.CONFIG_FILES</param-name>
		<param-value>
			/WEB-INF/faces-config.xml, /WEB-INF/faces-custom-config.xml,
			/WEB-INF/faces-commercialcurtailment-config.xml,
			/WEB-INF/faces-capcontrol-config.xml,
			/WEB-INF/faces-customer-config.xml
