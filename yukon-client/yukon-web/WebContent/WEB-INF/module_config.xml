<?xml version="1.0" encoding="ISO-8859-1"?>
<mainmenu>
<!-- 
     If you need to add functionality to the menu, please see the following classes:
       com.cannontech.web.menu.CommonMenuBuilder
       com.cannontech.web.menu.StandardMenuRenderer
       
     The <require***> tags can be used under the <option> and <module> elements.
     More than one tag can be specified if needed. The "value" attribute currently
     accepts either a fully qualified field name (e.g. com.cannontech.something.Class.PROPERTY)
     or a shortend version if the class is under com.cannontech.roles 
     (e.g. CBCSettingsRole.ACCESS).

     If you need to add a new "permission" types (like requireProperty) see:
       com.cannontech.web.menu.OptionPropertyChecker
       com.cannontech.web.menu.CommonMenuBuilder
       
     Be sure to use <link> for normal URLs (context path will be added)
     and <action> to call a JavaScript function via the javascript: protocol.
     
     CSS and JS files that should be included on every page of the module 
     can be declared with the <css file=""/> and <script file=""/> tags
     respectively.
-->
  <quicklinks>
    <option name="Home">
      <link>/home</link>
    </option>
    <option name="CapControl">
      <link>/capcontrol/subareas.jsp</link>
      <requireProperty value="capcontrol.CBCSettingsRole.ACCESS"/>
    </option>
    <option name="Commander">
      <link>/apps/SelectDevice.jsp</link>
      <requireRole value="application.CommanderRole.ROLEID"/>
    </option>
     <option name="Reporting">
      <link>/analysis/Reports.jsp</link>
      <requireRole value="application.ReportingRole.ROLEID"/>
    </option>
  </quicklinks>
  
  <module name="capcontrol">
    <toplinks>
      <option name="Create">
        <requireProperty value="capcontrol.CBCSettingsRole.CBC_DATABASE_EDIT"/>
        <sublinks>
          <option name="Substation">
            <link>/editor/cbcWizBase.jsf?type=4000</link>
          </option>
          <option name="Feeder">
            <link>/editor/cbcWizBase.jsf?type=4001</link>
          </option>
          <option name="CapBank">
            <link>/editor/cbcWizBase.jsf?type=1050</link>
          </option>
          <option name="CBC">
            <link>/editor/cbcWizBase.jsf?type=1051</link>
          </option>
          <option name="Schedule">
            <link>/editor/cbcWizBase.jsf?type=100000</link>
          </option>
        </sublinks>
      </option>
      <option name="Modify">
        <requireProperty value="capcontrol.CBCSettingsRole.CBC_DATABASE_EDIT"/>
        <sublinks>
          <option name="Edit">
            <script>editorPost()</script>
          </option>
          <option name="Delete">
            <script>deletePost()</script>
          </option>
        </sublinks>
      </option>
      <option name="Analysis">
        <requireFalseProperty value="capcontrol.CBCSettingsRole.HIDE_REPORTS"/>
        <sublinks>
          <option name="Today kVAR/Watt">
	        <requireFalseProperty value="capcontrol.CBCSettingsRole.HIDE_GRAPHS"/>
            <script>showPopUpChkBoxes('charts.jsp?type=VW&amp;period=Today')</script>
          </option>
          <option name="Today PF">
	        <requireFalseProperty value="capcontrol.CBCSettingsRole.HIDE_GRAPHS"/>
            <script>showPopUpChkBoxes('charts.jsp?type=PF&amp;period=Today')</script>
          </option>
          <option name="30 Day kVAR/Watt">
	        <requireFalseProperty value="capcontrol.CBCSettingsRole.HIDE_GRAPHS"/>
            <script>showPopUpChkBoxes('charts.jsp?type=VW')</script>
          </option>
          <option name="30 Day PF">
	        <requireFalseProperty value="capcontrol.CBCSettingsRole.HIDE_GRAPHS"/>
            <script>showPopUpChkBoxes('charts.jsp?type=PF')</script>
          </option>
        </sublinks>
      </option>
      <option name="View">
        <sublinks>
          <option name="Orphaned Feeders">
            <link>results.jsp?cbc_lastSearch=__cti_oFeeders__</link>
          </option>
          <option name="Orphaned CapBanks">
            <link>results.jsp?cbc_lastSearch=__cti_oBanks__</link>
          </option>
          <option name="Orphaned CBCs">
            <link>results.jsp?cbc_lastSearch=__cti_oCBCs__</link>
          </option>
          <option name="Schedules">
            <script>post('/editor/paoSchedListBase.jsf')</script>
          </option>
          <option name="One-Line">
	        <requireFalseProperty value="capcontrol.CBCSettingsRole.HIDE_ONELINE"/>
            <script>showConfirmWin(this)</script>
          </option>
          <option name="Recent Controls">
            <script>showRecentCmds('tabledata.jsp?type=RC')</script>
          </option>
        </sublinks>
      </option>
    </toplinks>
    <search action="results.jsp" method="post" fieldname="cbc_lastSearch"/>
    <skin name="standard"/>
    <css file="css/base.css"/> <!-- normally this should be an absolute path -->
    <css file="/WebConfig/yukon/styles/capcontrollogos.css"/>
  </module> 
  
  <module name="capcontrol_internal">
    <!-- The module is used for the inner frames that are used on the cap control pages. -->
    <skin name="inner"/>
    <css file="css/base.css"/> <!-- normally this should be an absolute path -->
  </module>
  
  <module name="commercialcurtailment">
    <toplinks>
      <option name="Events">
        <sublinks>
          <option name="Control">
            <link>/cc/programSelect.jsf</link>
          </option>
          <option name="Overview">
            <link>/cc/eventOverview.jsf</link>
          </option>
        </sublinks>
      </option>
      <option name="Setup">
        <sublinks>
          <option name="Programs">
            <link>/cc/programList.jsf</link>
          </option>
          <option name="Groups">
            <link>/cc/groupList.jsf</link>
          </option>
          <option name="Customers">
            <link>/cc/customerList.jsf</link>
          </option>
        </sublinks>
      </option>
    </toplinks>
    <css file="/cc/base.css"/>
    <css file="/WebConfig/yukon/styles/commcurtaillogos.css"/>
    <skin name="standard"/>
    <requireRole value="operator.CommercialMeteringRole.ROLEID"/>
  </module>
  
  <module name="commercialcurtailment_user">
    <toplinks>
      <option name="Control">
        <link>/cc/user/overview.jsf</link>
      </option>
      <option name="Trending">
        <link>/user/CILC/user_trending_sp.jsp</link>
      </option>
      <option name="Profile">
        <link>/user/CILC/user_ee_profile_sp.jsp</link>
      </option>
    </toplinks>
    <css file="/cc/base.css"/>
    <css file="/WebConfig/yukon/styles/commcurtaillogos.css"/>
    <skin name="standard"/>
    <requireRole value="cicustomer.CommercialMeteringRole.ROLEID"/>
  </module>
  
  <module name="esub">
  	<skin name="standard"/>
  	<css file="esub.css"/>
  </module>
  
  <module name="stars">
    <skin name="purple"/>
  </module>
    
   <module name="blank">
  </module>
</mainmenu>
