<?xml version="1.0" encoding="ISO-8859-1"?>
<mainmenu>
<!-- 
     If you need to add functionality to the menu, please see the following classes:
       com.cannontech.web.menu.CommonMenuBuilder
       com.cannontech.web.menu.StandardMenuRenderer
       
     If you need to add a new "permissions" type (like requireProperty) see:
       com.cannontech.web.menu.OptionPropertyChecker

     Be sure to use <link> for normal URLs (context path will be added)
     and <action> to call a JavaScript function via the javascript: protocol.
     
     CSS and JS files that should be included on every page of the module 
     can be declared with the <css file=""/> and <script file=""/> tags
     respectively.
-->
  <quicklinks>
    <option name="CapControl">
      <link>/capcontrol/subareas.jsp</link>
      <requireProperty value="com.cannontech.roles.capcontrol.CBCSettingsRole.ACCESS"/>
    </option>
    <option name="Commander">
      <link>/apps/SelectDevice.jsp</link>
      <requireRole value="com.cannontech.roles.application.CommanderRole.ROLEID"/>
    </option>
    <option name="Home">
      <link>/home</link>
    </option>
    <option name="Reporting">
      <link>/analysis/Reports.jsp</link>
      <requireRole value="com.cannontech.roles.application.ReportingRole.ROLEID"/>
    </option>
    
  </quicklinks>
  
  <module name="capcontrol">
    <toplinks>
      <option name="Create">
        <requireProperty value="com.cannontech.roles.capcontrol.CBCSettingsRole.CBC_DATABASE_EDIT"/>
        <sublinks>
          <option name="Substation">
            <link>/editor/cbcWizBase.jsf?type=4000</link>
          </option>
          <option name="Feeder">
            <link>/editor/cbcWizBase.jsf?type=4001</link>
          </option>
          <option name="CapBank">
            <link>/editor/cbcWizBase.jsf?type=1050</link>
          </option>
          <option name="CBC">
            <link>/editor/cbcWizBase.jsf?type=1051</link>
          </option>
          <option name="Schedule">
            <link>/editor/cbcWizBase.jsf?type=100000</link>
          </option>
        </sublinks>
      </option>
      <option name="Modify">
        <requireProperty value="com.cannontech.roles.capcontrol.CBCSettingsRole.CBC_DATABASE_EDIT"/>
        <sublinks>
          <option name="Edit">
            <script>editorPost()</script>
          </option>
          <option name="Delete">
            <script>deletePost()</script>
          </option>
          <option name="Copy">
            <script>copyPost()</script>
          </option>
        </sublinks>
      </option>
      <option name="Analysis">
        <requireFalseProperty value="com.cannontech.roles.capcontrol.CBCSettingsRole.HIDE_REPORTS"/>
        <sublinks>
          <option name="Today kVAR/Watt">
	        <requireFalseProperty value="com.cannontech.roles.capcontrol.CBCSettingsRole.HIDE_GRAPHS"/>
            <script>showPopUpChkBoxes('charts.jsp?type=VW&amp;period=Today')</script>
          </option>
          <option name="Today PF">
	        <requireFalseProperty value="com.cannontech.roles.capcontrol.CBCSettingsRole.HIDE_GRAPHS"/>
            <script>showPopUpChkBoxes('charts.jsp?type=PF&amp;period=Today')</script>
          </option>
          <option name="30 Day kVAR/Watt">
	        <requireFalseProperty value="com.cannontech.roles.capcontrol.CBCSettingsRole.HIDE_GRAPHS"/>
            <script>showPopUpChkBoxes('charts.jsp?type=VW')</script>
          </option>
          <option name="30 Day PF">
	        <requireFalseProperty value="com.cannontech.roles.capcontrol.CBCSettingsRole.HIDE_GRAPHS"/>
            <script>showPopUpChkBoxes('charts.jsp?type=PF')</script>
          </option>
        </sublinks>
      </option>
      <option name="View">
        <sublinks>
          <option name="Orphaned Feeders">
            <link>results.jsp?cbc_lastSearch=__cti_oFeeders__</link>
          </option>
          <option name="Orphaned CapBanks">
            <link>results.jsp?cbc_lastSearch=__cti_oBanks__</link>
          </option>
          <option name="Orphaned CBCs">
            <link>results.jsp?cbc_lastSearch=__cti_oCBCs__</link>
          </option>
          <option name="Schedules">
            <script>post('/editor/paoSchedListBase.jsf')</script>
          </option>
          <option name="One-Line">
	        <requireFalseProperty value="com.cannontech.roles.capcontrol.CBCSettingsRole.HIDE_ONELINE"/>
            <script>showConfirmWin(this)</script>
          </option>
          <option name="Recent Controls">
            <script>showRecentCmds('tabledata.jsp?type=RC')</script>
          </option>
        </sublinks>
      </option>
    </toplinks>
    <search action="results.jsp" method="post" fieldname="cbc_lastSearch"/>
    <skin name="standard"/>
    <css file="css/base.css"/> <!-- normally this should be an absolute path -->
    <css file="/WebConfig/yukon/styles/capcontrollogos.css"/>
  </module> 
  
  <module name="capcontrol_internal">
    <!-- The module is used for the inner frames that are used on the cap control pages. -->
    <skin name="inner"/>
    <css file="css/base.css"/> <!-- normally this should be an absolute path -->
  </module>
  
  <module name="commercialcurtailment">
    <toplinks>
      <option name="View">
        <sublinks>
          <option name="Programs">
            <link>/cc/programList.jsf</link>
          </option>
          <option name="Groups">
            <link>/cc/groupList.jsf</link>
          </option>
        </sublinks>
      </option>
    </toplinks>
    <skin name="standard"/>
  </module>
  
  <module name="esub">
  	<skin name="standard"/>
  	<css file="esub.css"/>
  </module>
  
  <module name="stars">
    <skin name="purple"/>
  </module>
    
   <module name="blank">
  </module>
</mainmenu>