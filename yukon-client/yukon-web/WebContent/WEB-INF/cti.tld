    <tag>
        <name>classUpdater</name>
        <tag-class>com.cannontech.web.taglib.ClassUpdaterTag</tag-class>
        <body-content>scriptless</body-content>
        <description>
        	Creates a span that updates it's class name.
        	Use by passing an updater type/identifier pair, or a full type+identifier key.
        	type/identifier pair will be used if they are passed in (over key if it also happens to be passed).
        	If a value is unavailable at render time, initialClassName will be used if passed.
        </description>
        <attribute>
            <name>type</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>identifier</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>key</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>initialClassName</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>elementType</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>elementType</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <description>
            	Type of element that will enclose the body of te tag. Defaults to 'span'.
            </description>
        </attribute>
        <attribute>
            <name>staticClass</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <description>
            	Class name that should always be on the element in addition to whatever class the updater appends.
            </description>
        </attribute>
    </tag>
    
