  <tag>
    <name>pickerProperties</name>
    <description>Retrieve properties of a picker.</description>
    <tag-class>com.cannontech.web.taglib.PickerPropertiesTag</tag-class>
    <body-content>empty</body-content>
    <attribute>
      <name>var</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
      <name>type</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The type of picker, i.e. 'commanderDevicePicker'</description>
    </attribute>
    <attribute>
      <name>property</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <description>The type of property to retrieve, i.e. 'ID_FIELD_NAME' or 'OUTPUT_COLUMNS'.</description>
    </attribute>
  </tag>
  
  <tag>
    <name>getBooleanConfigParam</name>
    <description>Retrieve a boolean config param. Returns false if param not specified.</description>
    <tag-class>com.cannontech.web.taglib.GetBooleanConfigParamTag</tag-class>
    <body-content>empty</body-content>
    <attribute>
      <name>param</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
      <name>var</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
      <name>scope</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
  </tag>
  
  <tag>
    <name>getMappingUrl</name>
    <description>Retrieve the mapping Url specified by the viewType (STREET, SATELLITE, HYBRID).</description>
    <tag-class>com.cannontech.web.taglib.GetMappingUrlTag</tag-class>
    <body-content>empty</body-content>
    <attribute>
      <name>viewType</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
  </tag>
  
