<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

    <bean id="starsInventoryActionController" class="com.cannontech.web.stars.action.StarsInventoryActionController" abstract="true">
        <property name="roleDao" ref="roleDao"/>
        <property name="authDao" ref="authDao"/>
        <property name="rolePropertyDao" ref="rolePropertyDao"/>
        <property name="contactDao" ref="contactDao"/>
        <property name="contactNotificationDao" ref="contactNotificationDao"/>
        <property name="dbPersistentDao" ref="dbPersistentDao" />
        <property name="energyCompanyDao" ref="energyCompanyDao"/>
        <property name="paoDao" ref="paoDao"/>
        <property name="starsDatabaseCache" ref="starsDatabaseCache"/>
        <property name="yukonListDao" ref="yukonListDao"/>
        <property name="yukonUserDao" ref="yukonUserDao"/>
        <property name="starsCustAccountInformationDao" ref="starsCustAccountInformationDao"/>
        <property name="inventoryBaseDao" ref="inventoryBaseDao" />
        <property name="starsSearchDao" ref="starsSearchDao" />
    </bean>

    <bean id="inventorySendSwitchCommands" class="com.cannontech.web.stars.action.inventory.SendSwitchCommandsController"
          parent="starsInventoryActionController"/>
    
    <bean id="inventoryRemoveSwitchCommands" class="com.cannontech.web.stars.action.inventory.RemoveSwitchCommandsController"
          parent="starsInventoryActionController"/>
    
    <bean id="inventoryFileWriteSwitchCommands" class="com.cannontech.web.stars.action.inventory.FileWriteSwitchCommandsController"
          parent="starsInventoryActionController"/>
    
    <bean id="inventoryStaticLoadGroupMap" class="com.cannontech.web.stars.action.inventory.StaticLoadGroupMapController"
          parent="starsInventoryActionController"/>
    
    <bean id="inventoryPurchaseChange" class="com.cannontech.web.stars.action.inventory.PurchaseChangeController"
          parent="starsInventoryActionController">
          <property name="jdbcTemplate" ref="simpleJdbcTemplate" />
    </bean>
    
    <bean id="inventoryRequestNewPurchasePlan" class="com.cannontech.web.stars.action.inventory.RequestNewPurchasePlanController"
          parent="starsInventoryActionController"/>
    
    <bean id="inventoryLoadPurchasePlan" class="com.cannontech.web.stars.action.inventory.LoadPurchasePlanController"
          parent="starsInventoryActionController"/>
    
    <bean id="inventoryDeletePurchasePlan" class="com.cannontech.web.stars.action.inventory.DeletePurchasePlanController"
          parent="starsInventoryActionController"/>
    
    <bean id="inventoryDeletePurchasePlanConfirmed" class="com.cannontech.web.stars.action.inventory.DeletePurchasePlanConfirmedController"
          parent="starsInventoryActionController"/>
    
    <bean id="inventoryRequestNewDeliverySchedule" class="com.cannontech.web.stars.action.inventory.RequestNewDeliveryScheduleController"
          parent="starsInventoryActionController"/>
    
    <bean id="inventoryLoadDeliverySchedule" class="com.cannontech.web.stars.action.inventory.LoadDeliveryScheduleController"
          parent="starsInventoryActionController"/>
    
    <bean id="inventoryDeliveryScheduleChange" class="com.cannontech.web.stars.action.inventory.DeliveryScheduleChangeController"
          parent="starsInventoryActionController"/>
    
    <bean id="inventoryRequestNewTimePeriod" class="com.cannontech.web.stars.action.inventory.RequestNewTimePeriodController"
          parent="starsInventoryActionController"/>
    
    <bean id="inventoryLoadTimePeriod" class="com.cannontech.web.stars.action.inventory.LoadTimePeriodController"
          parent="starsInventoryActionController"/>
    
    <bean id="inventoryTimePeriodChange" class="com.cannontech.web.stars.action.inventory.TimePeriodChangeController"
          parent="starsInventoryActionController"/>
    
    <bean id="inventoryRequestNewShipment" class="com.cannontech.web.stars.action.inventory.RequestNewShipmentController"
          parent="starsInventoryActionController"/>
    
    <bean id="inventoryLoadShipment" class="com.cannontech.web.stars.action.inventory.LoadShipmentController"
          parent="starsInventoryActionController"/>
    
    <bean id="inventoryShipmentChange" class="com.cannontech.web.stars.action.inventory.ShipmentChangeController"
          parent="starsInventoryActionController"/>
    
    <bean id="inventoryRequestNewInvoice" class="com.cannontech.web.stars.action.inventory.RequestNewInvoiceController"
          parent="starsInventoryActionController"/>
    
    <bean id="inventoryLoadInvoice" class="com.cannontech.web.stars.action.inventory.LoadInvoiceController"
          parent="starsInventoryActionController"/>

    <bean id="inventoryInvoiceChange" class="com.cannontech.web.stars.action.inventory.InvoiceChangeController"
          parent="starsInventoryActionController"/>
    
    <bean id="inventoryShipmentSNRangeAdd" class="com.cannontech.web.stars.action.inventory.ShipmentSNRangeAddController"
          parent="starsInventoryActionController"/>

</beans>