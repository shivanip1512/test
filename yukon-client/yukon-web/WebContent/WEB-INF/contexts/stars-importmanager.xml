<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

    <bean id="starsImportManagerController" class="com.cannontech.web.stars.action.StarsImportManagerActionController" abstract="true">
        <property name="authDao" ref="authDao"/>
        <property name="contactDao" ref="contactDao"/>
        <property name="contactNotificationDao" ref="contactNotificationDao"/>
        <property name="energyCompanyDao" ref="energyCompanyDao"/>
        <property name="paoDao" ref="paoDao"/>
        <property name="starsDatabaseCache" ref="starsDatabaseCache"/>
        <property name="yukonListDao" ref="yukonListDao"/>
        <property name="yukonUserDao" ref="yukonUserDao"/>
    </bean>

	<bean id="importmanagerImportCustAccounts" class="com.cannontech.web.stars.action.importmanager.ImportCustAccountsController"
          parent="starsImportManagerController"/>
	
	<bean id="importmanagerUploadGeneric" class="com.cannontech.web.stars.action.importmanager.UploadGenericController" 
          parent="starsImportManagerController"/>

    <bean id="importmanagerMigrateEnrollment" class="com.cannontech.web.stars.action.importmanager.MigrateEnrollmentController"
          parent="starsImportManagerController"/>
</beans>