<?xml version="1.0" encoding="ISO-8859-1" ?>

 <!DOCTYPE tiles-definitions PUBLIC
   "-//Apache Software Foundation//DTD Tiles Configuration 1.1//EN"
   "http://jakarta.apache.org/struts/dtds/tiles-config_1_1.dtd">


<tiles-definitions>

    <definition name=".baseLayout" path="/WEB-INF/layout/layout.jsp">
        <put name="title"  value="Yukon" />
        <put name="header" value="/WEB-INF/layout/header.jsp"/>
        <put name="content" value="{CONTENT}"/>
        <put name="footer" value="/WEB-INF/layout/footer.jsp"/>
    </definition>


    <definition name="deletePAO.tiles" extends=".baseLayout">
        <put name="title"  value="Delete Item(s)" />
		<put name="content" type="page" value="/WEB-INF/pages/deletePAO.jsp"/>
    </definition>

    <definition name="cbcEditor.tiles" extends=".baseLayout">
        <put name="title"  value="CapControl Editor" />
		<put name="content" type="page" value="/WEB-INF/pages/cbc/cbcEditor.jsp"/>
        <put name="header" value="/WEB-INF/pages/cbc/cbcHeader.jsp"/>
    </definition>

    <definition name="cbcWizard.tiles" extends="cbcEditor.tiles">
        <put name="title"  value="CapControl Wizard" />
		<put name="content" type="page" value="/WEB-INF/pages/cbc/cbcWizard.jsp"/>
    </definition>




    <definition name="ptEditor.tiles" extends=".baseLayout">
        <put name="title"  value="Point Editor" />
		<put name="content" type="page" value="/WEB-INF/pages/point/ptEditor.jsp"/>
    </definition>





</tiles-definitions>
